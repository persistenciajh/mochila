	<queryString>
		<![CDATA[SELECT            distinct
	TO_CHAR(MAX(calendario_nomina.fecha_inicia),'DD/MM/YYYY')     fecha_inicia,
	TO_CHAR(MAX(calendario_nomina.fecha_fin),'DD/MM/YYYY')         fecha_fin,
	TO_CHAR(MAX(calendario_nomina.fecha_pago),'DD/MM/YYYY')     fecha_pago,
	a.empresa,
	to_char(a.empleado)                                                       empleado,
	empleado.primer_apellido || ' ' ||
	empleado.segundo_apellido || ' ' ||
	empleado.nombre                                                                      nombre,
	ltrim(replace( to_char(empleado.nit,'999,999,999,999'),  ','   ,  '.') )      nit,
	empleado.centro_costo,
	centro_costo.nombre_centro_costo                                               nom_centro_costo,
	empleado.dependencia,
	d.nombre_dependencia,
	calendario_nomina.ano,
	to_char(calendario_nomina.numero_periodo)                        numero_periodo,
	to_char(calendario_nomina.orden_periodo)                         orden_periodo,
                      calendario_nomina.ano ||'-'|| calendario_nomina.numero_periodo||'-'|| calendario_nomina.orden_periodo  nomina,
                      cuenta.numero_cuenta cuenta,
                      valor_sueldo_rep (
		   $P{NOP_EMPRESA},
	   	   $P{NOP_EMPLEADO}
	                          )                                                                                 salario_basico,
                      ibc_empleado.valor                                                                                ibc,
                      apor_sal.salud                                                                                aporte_salud,
                      apor_pen.pension                                                                             aporte_pension,
                      c.nombre_cargo                                                                                     nombre_cargo,
                      tc.descripcion                                                                                        tipo_contrato ,
                      o.nombre_organizacion                                                                          nombre_organizacion,
                      o.direccion                                                                                            direccion_uni,
                      emp.nit                                                                                                 nit_universidad,
                    ( select pk_parametros_generales.valor('PVP')
                       texto from dual)                                                                        texto_param

   FROM
     (
     select nvl(descripcion,'EN CASO DE ENCONTRAR ALGUN MOTIVO DE RECLAMO EN LA PRESENTE LIQUIDACION, FAVOR FORMULARLO A DESARROLLO HUMANO DURANTE LOS TRES DIAS SIGUIENTES AL PAGO. DE LO CONTRARIO ENTENDEREMOS SU ACEPTACION.') texto
     from parametros_generales
     where codigo='PVP'
     )texto_parametro,
      (
      SELECT numero_cuenta, empleado
      FROM   empleado_entidad
      WHERE  empresa = $P{NOP_EMPRESA}
      AND    empleado = $P{NOP_EMPLEADO}
      AND    concepto = 999
      AND    estado = 'A'
      ) cuenta,
      (
     SELECT
                   SUM(VALOR) valor , empleado
     FROM      acumulado
     WHERE    empleado = $P{NOP_EMPLEADO}
     AND        concepto = pk_parametros_generales.valor('CIB')
     AND        ano||'-'||numero_periodo||'-'||orden_periodo=$P{NOP_PERIODO}
     AND        empresa = $P{NOP_EMPRESA}
     group by
             valor,
             empleado

    )ibc_empleado,
 (
      select     e_th.nombre_entidad      salud,
                   e.empleado                      empleado,
                   e.empresa                        empresa
     from       empleado e,
                   empleado_entidad ee,
                   entidad_th e_th
     WHERE   e.empresa = $P{NOP_EMPRESA}
     AND       e.empleado = $P{NOP_EMPLEADO}
     and        e.empleado=ee.empleado
     and       e.empresa=ee.empresa
     AND      ee.concepto = pk_parametros_generales.valor('CSA')
     and       e_th.tipo_entidad=ee.tipo_entidad
     and       e_th.entidad=ee.entidad
      )apor_sal,
 (
      select      e_th.nombre_entidad        pension, e.empleado,e.empresa
     from       empleado e,
                   empleado_entidad ee,
                   entidad_th e_th
     WHERE   e.empresa = $P{NOP_EMPRESA}
     AND       e.empleado = $P{NOP_EMPLEADO}
     and        e.empleado=ee.empleado
     and       e.empresa=ee.empresa
     AND      ee.concepto = pk_parametros_generales.valor('CPM')
     and       e_th.tipo_entidad=ee.tipo_entidad
     and       e_th.entidad=ee.entidad
      )apor_pen,
    calendario_nomina,
    acumulado               a,
    concepto,
    empleado,
    entidad_th,
    centro_costo,
    dependencia              d,
    cargo                         c,
    tipo_contrato              tc,
   organizacion                o,
  empresa                      emp
   WHERE   ibc_empleado.empleado (+)=empleado.empleado
   AND  cuenta.empleado(+)=a.empleado
   AND  calendario_nomina.empresa = $P{NOP_EMPRESA}--:P_EMPRESA
   AND (to_char(calendario_nomina.ano)||'-'||to_char(calendario_nomina.numero_periodo)||'-'||to_char(calendario_nomina.orden_periodo) = $P{NOP_PERIODO})
   AND calendario_nomina.periodo_concepto= 'MES'
   AND a.numero_periodo = calendario_nomina.numero_periodo
   AND a.ano = calendario_nomina.ano
   AND a.orden_periodo = calendario_nomina.orden_periodo
   AND a.empresa = calendario_nomina.empresa
   AND ( a.empresa, a.empleado ) IN (
                                SELECT DISTINCT
                                       a.empresa, a.empleado
				FROM   acumulado a
				WHERE  a.empresa = calendario_nomina.empresa
				AND    a.numero_periodo = calendario_nomina.numero_periodo
				AND    a.ano = calendario_nomina.ano
				AND    a.orden_periodo = calendario_nomina.orden_periodo
				AND    a.concepto = 999)
        AND  a.empleado = $P{NOP_EMPLEADO}
        AND  a.transaccion IN ('P','D')
        AND  concepto.concepto = a.concepto
        AND  empleado.empresa = a.empresa
        AND  empleado.empleado = a.empleado
        AND  entidad_th.tipo_entidad (+)= a.tipo_entidad
        AND  entidad_th.entidad (+)= a.entidad
        AND  centro_costo.organizacion = empleado.organizacion
        AND  centro_costo.centro_costo = empleado.centro_costo
        AND  d.dependencia (+)= empleado.dependencia
        AND  c.cargo(+)=empleado.cargo
        AND  c.grado(+)=empleado.grado
        AND  c.nivel(+)=empleado.nivel
        AND  tc.tipo_contrato=empleado.tipo_contrato
        AND  o.organizacion=a.organizacion
        AND  o.organizacion=emp.organizacion
        AND  a.empresa=emp.empresa
        AND  apor_sal.empleado(+)=a.empleado
        AND  apor_sal.empresa(+)=a.empresa
        AND  apor_pen.empleado(+)=a.empleado
        AND  apor_pen.empresa(+)=a.empresa
        and   rownum=1
 GROUP BY
	a.empresa,
	a.empleado,
	empleado.primer_apellido,
	empleado.segundo_apellido,
	empleado.nombre,
	empleado.nit,
	empleado.centro_costo,
	centro_costo.nombre_centro_costo    ,
	empleado.dependencia                    ,
	d.nombre_dependencia                    ,
	calendario_nomina.ano                     ,
	calendario_nomina.numero_periodo   ,
	calendario_nomina.orden_periodo     ,
                      cuenta.numero_cuenta                      ,
                      ibc_empleado.valor                          ,
	 apor_sal.salud                          ,
                      apor_pen.pension                       ,
                      c.nombre_cargo                               ,
                      tc.descripcion                                  ,
                      o.nombre_organizacion                    ,
                      o.direccion                                      ,
                      emp.nit                                          ,
                      texto_parametro.texto
 ORDER BY
        11,6]]>
	</queryString>
