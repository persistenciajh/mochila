	<queryString>
		<![CDATA[WITH TOTAL_GRUPO_CONTABLE AS
  (SELECT SDG.SEC_Grupo_Contable,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1
    || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) AS Saldo_Inicial_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1
    || $P{EFP_MES_BASE}), SDG.Debito, 0)) AS Debito_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1
    || $P{EFP_MES_BASE}), SDG.Credito, 0)) AS Credito_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}   - 1
    || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) AS Saldo_Final_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) AS Saldo_Inicial_Act,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || $P{EFP_MES_BASE}), SDG.Debito, 0)) AS Debito_Act,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || $P{EFP_MES_BASE}), SDG.Credito, 0)) AS Credito_Act,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) AS Saldo_Final_Act
  FROM COV_SALDO_DETALLADO_GRUPO SDG
  WHERE ((SDG.ANO = $P{EFP_ANO_BASE}
  AND SDG.MES     = $P{EFP_MES_BASE})
  OR (SDG.ANO     = $P{EFP_ANO_BASE} - 1
  AND SDG.MES     = $P{EFP_MES_BASE}) )
  GROUP BY SDG.SEC_Grupo_Contable
  ),
  DETALLE_RENGLON AS
  (SELECT ren.orden_estructura,
    ren.nivel,
    ren.sec_renglon,
    ren.mostrar_detalle,
    sdr.codigo_contable,
    sdr.unidad,
    sdr.fondo,
    sdr.fuente_funcion,
    sdr.secuencia_persona,
    SUM(sdr.saldo_inicial)                            AS saldo_inicial,
    SUM(sdr.debito)                                   AS debito,
    SUM(sdr.credito)                                  AS credito,
    SUM(sdr.saldo_inicial + sdr.debito - sdr.credito) AS saldo_final
  FROM efv_renglon ren
  INNER JOIN efv_saldo_detallado_renglon sdr
  ON sdr.sec_renglon              = ren.sec_renglon
  AND sdr.ano                     = $P{EFP_ANO_BASE}
  AND sdr.mes                     = $P{EFP_MES_BASE}
  WHERE ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
  GROUP BY ren.orden_estructura,
    ren.nivel,
    ren.sec_renglon,
    ren.mostrar_detalle,
    sdr.codigo_contable,
    sdr.unidad,
    sdr.fondo,
    sdr.fuente_funcion,
    sdr.secuencia_persona
  ),
  REVELACIONES AS
  (SELECT gcr.sec_renglon,
    npr.sec_nota_periodo_renglon,
    rev.sec_revelacion,
    rev.fecha,
    rev.revelacion,
    dco.centro_costo,
    dco.fondo,
    dco.secuencia_persona,
    dco.fuente_funcion,
    dco.codigo_contable,
    dco.valor_debito,
    dco.valor_credito
  FROM eft_nota_revelacion nre
  INNER JOIN cot_revelacion rev
  ON rev.sec_revelacion = nre.sec_revelacion
  INNER JOIN detalle_contabilidad dco
  ON dco.secuencia = rev.secuencia
  INNER JOIN cot_cuenta_grupo_contable cgc
  ON cgc.codigo_contable = dco.codigo_contable
  INNER JOIN eft_grupo_contable_renglon gcr
  ON gcr.sec_grupo_contable = cgc.sec_grupo_contable
  INNER JOIN eft_nota_periodo_renglon npr
  ON npr.sec_renglon = gcr.sec_renglon
  INNER JOIN efv_renglon ren
  ON ren.sec_renglon               = npr.sec_renglon
  WHERE ren.sec_estado_financiero  = $P{EFP_ESTADO_FINANCIERO}
  AND TO_CHAR(rev.fecha, 'YYYYMM') = $P{EFP_ANO_BASE}
    || $P{EFP_MES_BASE}
  AND npr.ano = $P{EFP_ANO_BASE}
  AND npr.mes = $P{EFP_MES_BASE}
  )
SELECT rst.grupo            AS aux_grupo,
  rst.orden                 AS aux_orden,
  efi.sec_estado_financiero AS sec_estado_financiero,
  (
  CASE rst.grupo
    WHEN 1
    THEN efi.estado_financiero
    ELSE NULL
  END)            AS estado_financiero,
  efi.nombre      AS nombre_estado_financiero,
  rst.sec_renglon AS sec_renglon,
  (
  CASE rst.grupo
    WHEN 1
    THEN ren.renglon
    ELSE NULL
  END) AS renglon,
  (
  CASE rst.grupo
    WHEN 1
    THEN ren.detalla_anexo
    ELSE NULL
  END)                         AS detalla_anexo,
  rst.sec_nota_periodo_renglon AS sec_nota_periodo_renglon,
  rst.sec_revelacion,
  (
  CASE rst.grupo
    WHEN 1
    THEN npr.nota_periodo
    WHEN 2
    THEN npr.nota_periodo
    ELSE NULL
  END) AS nota_periodo,
  (
  CASE rst.grupo
    WHEN 2
    THEN npr.descripcion
    ELSE NULL
  END)                                                                                                                                                                                                                                              AS descripcion_nota,
  rst.CODIGO_NIVEL_3                                                                                                                                                                                                                                AS CODIGO_NIVEL_3,
  rst.NOMBRE_NIVEL_3                                                                                                                                                                                                                                AS NOMBRE_NIVEL_3,
  rst.CODIGO_NIVEL_4                                                                                                                                                                                                                                AS CODIGO_NIVEL_4,
  rst.NOMBRE_NIVEL_4                                                                                                                                                                                                                                AS NOMBRE_NIVEL_4,
  rst.CODIGO_NIVEL_5                                                                                                                                                                                                                                AS CODIGO_NIVEL_5,
  rst.NOMBRE_NIVEL_5                                                                                                                                                                                                                                AS NOMBRE_NIVEL_5,
  to_number(DECODE(rst.grupo, 2, NULL, DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.saldo_inicial/1000, 'M', rst.saldo_inicial/1000000, rst.saldo_inicial)))                                                                                        AS saldo_inicial,
  to_number(DECODE(rst.grupo, 2, NULL, 3, DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.debito    /1000, 'M', rst.debito/1000000, rst.debito), DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.debito/1000, 'M', rst.debito/1000000, rst.debito)))      AS debito,
  to_number(DECODE(rst.grupo, 2, NULL, 3, DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.credito   /1000, 'M', rst.credito/1000000, rst.credito), DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.credito/1000, 'M', rst.credito/1000000, rst.credito))) AS credito,
  to_number(DECODE(rst.grupo, 2, NULL, DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.saldo_final  /1000, 'M', rst.saldo_final/1000000, rst.saldo_final)))                                                                                            AS saldo_final,
  to_number(DECODE(rst.grupo, 2, NULL, DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.valor        /1000, 'M', rst.valor/1000000, rst.valor)))                                                                                                        AS valor,
  eim.negrilla                                                                                                                                                                                                                                      AS aux_negrilla,
  eim.italica                                                                                                                                                                                                                                       AS aux_italica,
  eim.subrayado                                                                                                                                                                                                                                     AS aux_subrayado,
  eim.tamano_caracter                                                                                                                                                                                                                               AS aux_tamano_caracter
FROM
  (SELECT -- RENGLON
    1 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 1)                        AS ORDEN,
    NPR.SEC_RENGLON              AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
    NULL                         AS SEC_REVELACION,
    NULL                         AS CODIGO_NIVEL_3,
    NULL                         AS NOMBRE_NIVEL_3,
    NULL                         AS CODIGO_NIVEL_4,
    NULL                         AS NOMBRE_NIVEL_4,
    NULL                         AS CODIGO_NIVEL_5,
    NULL                         AS NOMBRE_NIVEL_5,
    NULL                         AS SALDO_INICIAL,
    NULL                         AS DEBITO,
    NULL                         AS CREDITO,
    NULL                         AS SALDO_FINAL,
    NVL(TRA.TOTAL_RENGLON, 0)    AS VALOR
  FROM EFV_RENGLON REN
  LEFT OUTER JOIN
    (SELECT REN.SEC_RENGLON,
      SUM(TRE.TOTAL_RENGLON) AS TOTAL_RENGLON
    FROM EFV_RENGLON REN
    INNER JOIN EFT_RENGLON_J JER
    ON JER.SEC_RENGLON = REN.SEC_RENGLON
    LEFT OUTER JOIN
      (
      SELECT RGV.SEC_RENGLON,
        SUM(DECODE(RGV.CAMBIO_SIGNO, 'S', RGV.TOTAL_GRUPO * -1, RGV.TOTAL_GRUPO)) AS TOTAL_RENGLON
      FROM
        (SELECT GCR.SEC_RENGLON,
          TGC.SEC_GRUPO_CONTABLE,
          GCO.CAMBIO_SIGNO,
          SUM(DECODE(GCR.TIPO_VALOR, 'SC', TGC.SALDO_FINAL_ACT, 'VR', TGC.SALDO_FINAL_ACT - TGC.SALDO_FINAL_ANT, 'MD', TGC.DEBITO_ACT, 'MC', TGC.CREDITO_ACT, 'VA', TGC.DEBITO_ACT - TGC.CREDITO_ACT, 'VP', TGC.CREDITO_ACT - TGC.DEBITO_ACT, 'SA', TGC.SALDO_FINAL_ANT ) ) AS TOTAL_GRUPO
        FROM TOTAL_GRUPO_CONTABLE TGC
        INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
        ON GCR.SEC_GRUPO_CONTABLE = TGC.SEC_GRUPO_CONTABLE
        INNER JOIN COT_GRUPO_CONTABLE GCO
        ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
        GROUP BY GCR.SEC_RENGLON,
          TGC.SEC_GRUPO_CONTABLE,
          GCO.CAMBIO_SIGNO
        ) RGV
      GROUP BY RGV.SEC_RENGLON
      ) TRE ON TRE.SEC_RENGLON = JER.SEC_RENGLON
    GROUP BY REN.SEC_RENGLON
    ) TRA ON TRA.SEC_RENGLON = REN.SEC_RENGLON
  LEFT OUTER JOIN EFT_NOTA_PERIODO_RENGLON NPR
  ON NPR.SEC_RENGLON              = REN.SEC_RENGLON
  WHERE REN.SEC_ESTADO_FINANCIERO = $P{EFP_ESTADO_FINANCIERO}
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  UNION ALL
  SELECT --NOTA PRINCIPAL
    2 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 2)                        AS ORDEN,
    NPR.SEC_RENGLON              AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
    NULL                         AS SEC_REVELACION,
    NULL                         AS CODIGO_NIVEL_3,
    NULL                         AS NOMBRE_NIVEL_3,
    NULL                         AS CODIGO_NIVEL_4,
    NULL                         AS NOMBRE_NIVEL_4,
    NULL                         AS CODIGO_NIVEL_5,
    NULL                         AS NOMBRE_NIVEL_5,
    NULL                         AS SALDO_INICIAL,
    NULL                         AS DEBITO,
    NULL                         AS CREDITO,
    NULL                         AS SALDO_FINAL,
    NULL                         AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  WHERE NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  UNION ALL
  SELECT -- REVELACIONES
    3 AS GRUPO,
    TO_CHAR(REV.SEC_RENGLON
    || REV.SEC_NOTA_PERIODO_RENGLON
    || 3
    || REV.SEC_REVELACION)       AS ORDEN,
    REV.SEC_RENGLON              AS SEC_RENGLON,
    REV.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
    REV.SEC_REVELACION           AS SEC_REVELACION,
    ('No. '
    || REV.SEC_REVELACION
    || ' - '
    || TO_CHAR(REV.FECHA, 'DD/MM/YYYY')) AS CODIGO_NIVEL_3,
    REV.REVELACION                       AS NOMBRE_NIVEL_3,
    NULL                                 AS CODIGO_NIVEL_4,
    NULL                                 AS NOMBRE_NIVEL_4,
    NULL                                 AS CODIGO_NIVEL_5,
    NULL                                 AS NOMBRE_NIVEL_5,
    NULL                                 AS SALDO_INICIAL,
    REV.VALOR_DEBITO                     AS DEBITO,
    REV.VALOR_CREDITO                    AS CREDITO,
    NULL                                 AS SALDO_FINAL,
    NULL                                 AS VALOR
  FROM REVELACIONES REV
  WHERE REV.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  UNION ALL
  SELECT -- RENGLON - CUENTA
    4 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 4
    || DRE.CODIGO_CONTABLE)      AS ORDEN,
    NPR.SEC_RENGLON              AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
    NULL                         AS SEC_REVELACION,
    NULL                         AS CODIGO_NIVEL_3,
    NULL                         AS NOMBRE_NIVEL_3,
    DRE.CODIGO_CONTABLE          AS CODIGO_NIVEL_4,
    PCO.NOMBRE_CUENTA            AS NOMBRE_NIVEL_4,
    NULL                         AS CODIGO_NIVEL_5,
    NULL                         AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)       AS SALDO_INICIAL,
    SUM(DRE.DEBITO)              AS DEBITO,
    SUM(DRE.CREDITO)             AS CREDITO,
    SUM(DRE.SALDO_FINAL)         AS SALDO_FINAL,
    NULL                         AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN PLAN_CONTABLE PCO
  ON PCO.CODIGO_CONTABLE     = DRE.CODIGO_CONTABLE
  WHERE ((REN.DETALLA_ANEXO <> 'UNI')
  AND (REN.DETALLA_ANEXO    <> 'UGC'))
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.CODIGO_CONTABLE,
    PCO.NOMBRE_CUENTA
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  UNION ALL
  SELECT -- RENGLON - UNIDAD
    4 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 4
    || DRE.UNIDAD)               AS ORDEN,
    NPR.SEC_RENGLON              AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
    NULL                         AS SEC_REVELACION,
    NULL                         AS CODIGO_NIVEL_3,
    NULL                         AS NOMBRE_NIVEL_3,
    DRE.UNIDAD                   AS CODIGO_NIVEL_4,
    CCO.NOMBRE_CENTRO_COSTO      AS NOMBRE_NIVEL_4,
    NULL                         AS CODIGO_NIVEL_5,
    NULL                         AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)       AS SALDO_INICIAL,
    SUM(DRE.DEBITO)              AS DEBITO,
    SUM(DRE.CREDITO)             AS CREDITO,
    SUM(DRE.SALDO_FINAL)         AS SALDO_FINAL,
    NULL                         AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN CENTRO_COSTO CCO
  ON CCO.CENTRO_COSTO       = DRE.UNIDAD
  WHERE ((REN.DETALLA_ANEXO = 'UNI')
  AND (REN.DETALLA_ANEXO    = 'UGC'))
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.UNIDAD,
    CCO.NOMBRE_CENTRO_COSTO
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  UNION ALL
  SELECT -- RENGLON - CUENTA - FONDO
    5 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 5
    || DRE.CODIGO_CONTABLE
    || DRE.FONDO)                            AS ORDEN,
    NPR.SEC_RENGLON                          AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON             AS SEC_NOTA_PERIODO_RENGLON,
    NULL                                     AS SEC_REVELACION,
    NULL                                     AS CODIGO_NIVEL_3,
    NULL                                     AS NOMBRE_NIVEL_3,
    DRE.CODIGO_CONTABLE                      AS CODIGO_NIVEL_4,
    NULL                                     AS NOMBRE_NIVEL_4,
    NVL(TO_CHAR(DRE.FONDO), 'SFO')           AS CODIGO_NIVEL_5,
    NVL(FON.NOMBRE_FONDO, '-=|SIN FONDO|=-') AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)                   AS SALDO_INICIAL,
    SUM(DRE.DEBITO)                          AS DEBITO,
    SUM(DRE.CREDITO)                         AS CREDITO,
    SUM(DRE.SALDO_FINAL)                     AS SALDO_FINAL,
    NULL                                     AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  LEFT OUTER JOIN FONDO FON
  ON FON.FONDO            = DRE.FONDO
  WHERE REN.DETALLA_ANEXO = 'CFO'
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.CODIGO_CONTABLE,
    DRE.FONDO,
    FON.NOMBRE_FONDO
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  UNION ALL
  SELECT -- RENGLON - CUENTA - FUENTE FUNCION
    5 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 5
    || DRE.CODIGO_CONTABLE
    || DRE.FUENTE_FUNCION)                                     AS ORDEN,
    NPR.SEC_RENGLON                                            AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON                               AS SEC_NOTA_PERIODO_RENGLON,
    NULL                                                       AS SEC_REVELACION,
    NULL                                                       AS CODIGO_NIVEL_3,
    NULL                                                       AS NOMBRE_NIVEL_3,
    DRE.CODIGO_CONTABLE                                        AS CODIGO_NIVEL_4,
    NULL                                                       AS NOMBRE_NIVEL_4,
    NVL(TO_CHAR(DRE.FUENTE_FUNCION), 'SFF')                    AS CODIGO_NIVEL_5,
    NVL(FFU.NOMBRE_FUENTE_FUNCION, '-=|SIN FUENTE FUNCION|=-') AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)                                     AS SALDO_INICIAL,
    SUM(DRE.DEBITO)                                            AS DEBITO,
    SUM(DRE.CREDITO)                                           AS CREDITO,
    SUM(DRE.SALDO_FINAL)                                       AS SALDO_FINAL,
    NULL                                                       AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  LEFT OUTER JOIN FUENTE_FUNCION FFU
  ON FFU.FUENTE_FUNCION   = DRE.FUENTE_FUNCION
  WHERE REN.DETALLA_ANEXO = 'CFF'
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.CODIGO_CONTABLE,
    DRE.FUENTE_FUNCION,
    FFU.NOMBRE_FUENTE_FUNCION
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  UNION ALL
  SELECT -- RENGLON - CUENTA - TERCERO
    5 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 5
    || DRE.CODIGO_CONTABLE
    || PER.IDENTIFICACION)                  AS ORDEN,
    NPR.SEC_RENGLON                         AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON            AS SEC_NOTA_PERIODO_RENGLON,
    NULL                                    AS SEC_REVELACION,
    NULL                                    AS CODIGO_NIVEL_3,
    NULL                                    AS NOMBRE_NIVEL_3,
    DRE.CODIGO_CONTABLE                     AS CODIGO_NIVEL_4,
    NULL                                    AS NOMBRE_NIVEL_4,
    NVL(TO_CHAR(PER.IDENTIFICACION), 'STE') AS CODIGO_NIVEL_5,
    NVL((TRIM(PER.NOMBRE_RAZON_SOCIAL
    || ' '
    || NVL(PER.PRIMER_APELLIDO,'')
    || ' '
    || NVL(PER.SEGUNDO_APELLIDO,''))), 'SIN TERCERO') AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)                            AS SALDO_INICIAL,
    SUM(DRE.DEBITO)                                   AS DEBITO,
    SUM(DRE.CREDITO)                                  AS CREDITO,
    SUM(DRE.SALDO_FINAL)                              AS SALDO_FINAL,
    NULL                                              AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  LEFT OUTER JOIN PERSONA PER
  ON PER.SECUENCIA_PERSONA = DRE.SECUENCIA_PERSONA
  WHERE REN.DETALLA_ANEXO  = 'CTE'
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.CODIGO_CONTABLE,
    PER.IDENTIFICACION,
    PER.NOMBRE_RAZON_SOCIAL,
    PER.PRIMER_APELLIDO,
    PER.SEGUNDO_APELLIDO
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  UNION ALL
  SELECT -- RENGLON - CUENTA - UNIDAD
    5 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 5
    || DRE.CODIGO_CONTABLE
    || DRE.UNIDAD)                                   AS ORDEN,
    NPR.SEC_RENGLON                                  AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON                     AS SEC_NOTA_PERIODO_RENGLON,
    NULL                                             AS SEC_REVELACION,
    NULL                                             AS CODIGO_NIVEL_3,
    NULL                                             AS NOMBRE_NIVEL_3,
    DRE.CODIGO_CONTABLE                              AS CODIGO_NIVEL_4,
    NULL                                             AS NOMBRE_NIVEL_4,
    NVL(TO_CHAR(DRE.UNIDAD), 'SUN')                  AS CODIGO_NIVEL_5,
    NVL(CCO.NOMBRE_CENTRO_COSTO, '-=|SIN UNIDAD|=-') AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)                           AS SALDO_INICIAL,
    SUM(DRE.DEBITO)                                  AS DEBITO,
    SUM(DRE.CREDITO)                                 AS CREDITO,
    SUM(DRE.SALDO_FINAL)                             AS SALDO_FINAL,
    NULL                                             AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  LEFT OUTER JOIN CENTRO_COSTO CCO
  ON CCO.CENTRO_COSTO     = DRE.UNIDAD
  WHERE REN.DETALLA_ANEXO = 'CUN'
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.CODIGO_CONTABLE,
    DRE.UNIDAD,
    CCO.NOMBRE_CENTRO_COSTO
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  UNION ALL
  SELECT -- RENGLON - UNIDAD - GRUPO CONTABLE
    5 AS GRUPO,
    TO_CHAR(NPR.SEC_RENGLON
    || NPR.SEC_NOTA_PERIODO_RENGLON
    || 5
    || DRE.UNIDAD)                  AS ORDEN,
    NPR.SEC_RENGLON                 AS SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON    AS SEC_NOTA_PERIODO_RENGLON,
    NULL                            AS SEC_REVELACION,
    NULL                            AS CODIGO_NIVEL_3,
    NULL                            AS NOMBRE_NIVEL_3,
    DRE.UNIDAD                      AS CODIGO_NIVEL_4,
    NULL                            AS NOMBRE_NIVEL_4,
    TO_CHAR(GCR.SEC_GRUPO_CONTABLE) AS CODIGO_NIVEL_5,
    GCO.NOMBRE                      AS NOMBRE_NIVEL_5,
    SUM(DRE.SALDO_INICIAL)          AS SALDO_INICIAL,
    SUM(DRE.DEBITO)                 AS DEBITO,
    SUM(DRE.CREDITO)                AS CREDITO,
    SUM(DRE.SALDO_FINAL)            AS SALDO_FINAL,
    NULL                            AS VALOR
  FROM EFT_NOTA_PERIODO_RENGLON NPR
  INNER JOIN EFV_RENGLON REN
  ON REN.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN DETALLE_RENGLON DRE
  ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
  INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
  ON GCR.SEC_RENGLON = DRE.SEC_RENGLON
  INNER JOIN COT_GRUPO_CONTABLE GCO
  ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
  INNER JOIN COT_CUENTA_GRUPO_CONTABLE CGC
  ON CGC.SEC_GRUPO_CONTABLE = GCO.SEC_GRUPO_CONTABLE
  AND CGC.CODIGO_CONTABLE   = DRE.CODIGO_CONTABLE
  WHERE REN.DETALLA_ANEXO   = 'UGC'
  AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
  AND NPR.ANO = $P{EFP_ANO_BASE}
  AND NPR.MES = $P{EFP_MES_BASE}
  GROUP BY NPR.SEC_RENGLON,
    NPR.SEC_NOTA_PERIODO_RENGLON,
    DRE.UNIDAD,
    GCR.SEC_GRUPO_CONTABLE,
    GCO.NOMBRE
  HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
  ) rst
INNER JOIN eft_renglon ren
ON ren.sec_renglon = rst.sec_renglon
INNER JOIN eft_estado_financiero efi
ON efi.sec_estado_financiero = ren.sec_estado_financiero
INNER JOIN eft_nota_periodo_renglon npr
ON npr.sec_nota_periodo_renglon = rst.sec_nota_periodo_renglon
LEFT OUTER JOIN eft_efecto_impresion eim
ON eim.sec_efecto_impresion     = ren.sec_efecto_impresion
WHERE efi.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
ORDER BY rst.orden,
  rst.CODIGO_NIVEL_3,
  rst.CODIGO_NIVEL_4,
  rst.CODIGO_NIVEL_5]]>
	</queryString>
	<field name="AUX_GRUPO" class="java.math.BigDecimal"/>
	<field name="AUX_ORDEN" class="java.lang.String"/>
	<field name="SEC_ESTADO_FINANCIERO" class="java.math.BigDecimal"/>
