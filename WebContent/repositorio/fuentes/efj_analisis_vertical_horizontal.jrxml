		<queryString>
			<![CDATA[select frd.orden,
  TO_CHAR(frd.firma) as empleado
from get_firma_reporte fre
  inner join get_firma_reporte_detalle frd
    on frd.sec_firma_reporte = fre.sec_firma_reporte
  inner join get_componente gc
    on fre.sec_componente = gc.sec_componente
  inner join eft_estado_financiero efi
    on efi.sec_firma_reporte = fre.sec_firma_reporte
where gc.componente = $P{GEP_COMPONENTE}
  and efi.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
order by frd.orden]]>
		</queryString>
