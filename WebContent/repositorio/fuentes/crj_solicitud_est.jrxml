	<queryString>
		<![CDATA[SELECT
sol.sec_solicitud AS SEC_SOLICITUD,
sol.credito AS CREDITO,
per.tipo_identificacion AS TIPO_IDENTIFICACION,
per.identificacion AS IDENTIFICACION_PERSONA,
(per.nombre_razon_social||' '||per.primer_apellido||' '||per.segundo_apellido) AS NOMBRE_PERSONA,
DECODE(sol.estado,'G','GRABADO','E','ESTUDIO','A','APROBADO','R','RECHAZADO') AS ESTADO_SOLICITUD,
sol.valor_financiacion AS VALOR_FINANCIACION,
sol.valor_pago_inicial AS VALOR_PAGO_INICIAL,
lc.descripcion AS DESCRIPCION_LC,
lc.porcentaje AS PORCENTAJE,
lc.porcentaje_mora AS PORCENTAJE_MORA,
DECODE(lc.tipo_pago_inicial,'V','VALOR','P','PORCENTAJE') AS TIPO_PAGO_INICIAL,
lc.plazo AS NUMERO_MAXIMO_CUOTAS,
sol.cuotas AS NUMERO_CUOTAS_SOLICITUD,
DECODE(lc.pago_interes_anticipado,'S','TOTAL ANTICIPADO','N','ANUALIDAD VENCIDA','T','ANUALIDAD ANTICIPADA','F','CAPITAL FIJO INT. VENCIDO', 'A','CAPITAL FIJO INT. ANTICIPADO') AS PAGO_INTERES_ANTICIPADO,
DECODE(lc.modalidad_pago,'C','CHEQUE','P','PAGARE','O','OTRO') AS MODALIDAD_PAGO ,
decode(lc.tipo_linea,'L','LIBRE','M','MATRICULA','C','CARTERA','P','PRODUCTO') AS TIPO_LINEA,
trunc( sol.fecha_solicitud) AS FECHA_SOLICITUD,
cs.cuota AS CUOTA,
cs.fecha_vencimiento AS FECHA_VENCIMIENTO,
sol.fecha_uso AS FECHA_USO,
e.nombre_entidad AS ENTIDAD,
ds.tipo_documento AS TIPO_DOCUMENTO,
ds.numero_documento AS NUMERO_DOCUMENTO,
ds.numero_cuenta AS NUMERO_CUENTA,
SUM(decode(consol.concepto,'C',consol.valor,0)) AS CAPITAL,
SUM(decode(consol.concepto,'I',consol.valor,0)) AS INTERES,
SUM(decode(consol.concepto,'G',consol.valor,0)) AS GASTOS,
cs.total AS VALOR_CUOTA,
sol.cliente AS CLIENTE
FROM crt_solicitud sol
INNER JOIN crt_cuota_solicitud cs on cs.sec_solicitud = sol.sec_solicitud
INNER JOIN crt_concepto_solicitud consol on consol.sec_cuota_solicitud = cs.sec_cuota_solicitud
INNER JOIN linea_credito lc on lc.linea_credito = sol.linea_credito
INNER JOIN cliente cl on cl.cliente = sol.cliente
INNER JOIN persona per on per.secuencia_persona = cl.secuencia_persona
LEFT OUTER JOIN crt_respaldo_solicitud rs on cs.sec_cuota_solicitud = rs.sec_cuota_solicitud
LEFT OUTER JOIN crt_documento_solicitud ds on ds.sec_documento_solicitud = rs.sec_documento_solicitud
LEFT OUTER JOIN entidad e on e.entidad = ds.entidad_girador
AND e.tipo_entidad = ds.tipo_entidad_girador
WHERE sol.sec_solicitud = nvl($P{SEC_SOLICITUD},sol.sec_solicitud)
and to_char(per.identificacion) like nvl(to_char($P{IDENTIFICACION}),per.identificacion)
and cl.cliente = nvl($P{SEC_CLIENTE},cl.cliente)
and ( sol.estado in ('R','A') and sol.estado = nvl($P{ESTADO},sol.estado) )
GROUP BY sol.sec_solicitud, sol.credito, per.tipo_identificacion, per.identificacion, (per.nombre_razon_social||' '||per.primer_apellido||' '|| per.segundo_apellido), DECODE(sol.estado,'G','GRABADO','E','ESTUDIO','A','APROBADO','R','RECHAZADO'), sol.valor_financiacion, sol.valor_pago_inicial, lc.descripcion, lc.porcentaje, lc.porcentaje_mora, DECODE(lc.tipo_pago_inicial,'V','VALOR','P','PORCENTAJE'), lc.plazo, sol.cuotas, DECODE(lc.pago_interes_anticipado,'S','TOTAL ANTICIPADO','N','ANUALIDAD VENCIDA','T','ANUALIDAD ANTICIPADA','F','CAPITAL FIJO INT. VENCIDO', 'A','CAPITAL FIJO INT. ANTICIPADO'), DECODE(lc.modalidad_pago,'C','CHEQUE','P','PAGARE','O','OTRO'), decode(lc.tipo_linea,'L','LIBRE','M','MATRICULA','C','CARTERA','P','PRODUCTO'), trunc( sol.fecha_solicitud), cs.cuota, cs.fecha_vencimiento,sol.fecha_uso, e.nombre_entidad, ds.tipo_documento, ds.numero_documento, ds.numero_cuenta, cs.total,sol.cliente
ORDER BY sol.sec_solicitud, cs.cuota]]>
	</queryString>
