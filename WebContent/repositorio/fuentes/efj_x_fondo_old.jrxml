	<queryString>
		<![CDATA[select
    trc.grupo,
    trc.orden_estructura,
    decode(trc.grupo, 0, trc.sec_estado_financiero, null) as sec_estado_financiero,
    decode(trc.grupo, 0, efi.estado_financiero, null) as estado_financiero,
    decode(trc.grupo, 0, efi.nombre, null) as nombre_estado_financiero,
    decode(trc.grupo, 1, trc.sec_renglon, null) as sec_renglon,
    decode(trc.grupo, 1, ren.renglon, null) as renglon,
    decode(trc.grupo, 1, ren.separador, 2, ren.separador, null) as separador,
    decode(trc.grupo, 1, ren.origen_valor, 2, ren.origen_valor, null) as origen_valor,
    decode(trc.grupo, 1, ren.tipo_saldo, 2, ren.tipo_saldo, null) as tipo_saldo,
    decode(trc.grupo, 1, ren.mostrar_saldo, 2, ren.mostrar_saldo, null) as mostrar_saldo,
    decode(trc.grupo, 1, ren.cambio_signo, 2, ren.cambio_signo, null) as cambio_signo,
    decode(trc.grupo, 1, ren.nivel, 2, ren.nivel+1, null) as aux_nivel,
    decode(trc.grupo, 1, npr.nota_periodo, null) as nota_periodo,
    decode(trc.grupo, 2, trc.fondo, null) as fondo,
    decode(trc.grupo, 2, fon.nombre_fondo, null) as nombre_fondo,
    to_number(decode(trc.grupo, 0, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', trc.saldo_inicial_mes/1000, 'M', trc.saldo_inicial_mes/1000000, trc.saldo_inicial_mes))) as saldo_inicial_base,
    to_number(decode(trc.grupo, 0, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', trc.debito_mes/1000, 'M', trc.debito_mes/1000000, trc.debito_mes))) as debito_base,
    to_number(decode(trc.grupo, 0, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', trc.credito_mes/1000, 'M', trc.credito_mes/1000000, trc.credito_mes))) as credito_base,
    to_number(decode(trc.grupo, 0, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', trc.saldo_final_mes/1000, 'M', trc.saldo_final_mes/1000000, trc.saldo_final_mes))) as saldo_final_base,
    decode(trc.grupo, 1, eim.negrilla, null) as aux_negrilla,
    decode(trc.grupo, 1, eim.italica, null) as aux_italica,
    decode(trc.grupo, 1, eim.subrayado, null) as aux_subrayado,
    decode(trc.grupo, 1, eim.tamano_caracter, null) as aux_tamano
from (
        select distinct
            0 as grupo,
            0 as orden_estructura,
            ren.sec_estado_financiero as sec_estado_financiero,
            null as sec_renglon,
            null as fondo,
            null as saldo_inicial_mes,
            null as debito_mes,
            null as credito_mes,
            null as saldo_final_mes
        from efv_renglon ren
        where ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}

        union all

        select
            1 as grupo,
            ren.orden_estructura,
            ren.sec_estado_financiero as sec_estado_financiero,
            ren.sec_renglon,
            null as fondo,
            nvl(trm.saldo_inicial,0) as saldo_inicial_mes,
            nvl(trm.debito,0) as debito_mes,
            nvl(trm.credito,0) as credito_mes,
            nvl((trm.saldo_inicial + trm.debito - trm.credito),0) as saldo_final_mes
        from efv_renglon ren
            left outer join efv_total_renglon_mes trm
                on trm.sec_renglon = ren.sec_renglon
                    and trm.ano = $P{EFP_ANO_BASE}
                        and trm.mes = $P{EFP_MES_BASE}
        where ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}

        union all

        select
            2 grupo,
            ren.orden_estructura,
            ren.sec_estado_financiero as sec_estado_financiero,
            ren.sec_renglon,
            sdr.fondo,
            sum(sdr.saldo_inicial) as saldo_inicial_mes,
            sum(sdr.debito) as debito_mes,
            sum(sdr.credito) as credito_mes,
            (sum(sdr.saldo_inicial) + sum(sdr.debito) - sum(sdr.credito)) as saldo_final_mes
        from efv_renglon ren
            inner join efv_saldo_detallado_renglon sdr
                on sdr.sec_renglon = ren.sec_renglon
                    and sdr.ano = $P{EFP_ANO_BASE}
                        and sdr.mes = $P{EFP_MES_BASE}
        where ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
        group by ren.orden_estructura, ren.sec_estado_financiero, ren.sec_renglon, sdr.fondo
        ) trc
        inner join eft_estado_financiero efi
            on efi.sec_estado_financiero = trc.sec_estado_financiero
        left outer join efv_renglon ren
            on ren.sec_renglon = trc.sec_renglon
        left outer join eft_efecto_impresion eim
            on eim.sec_efecto_impresion = ren.sec_efecto_impresion
        left outer join fondo fon
            on fon.fondo = trc.fondo
        left outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = trc.sec_renglon
                and npr.ano = $P{EFP_ANO_BASE}
                    and npr.mes = $P{EFP_MES_BASE}
where ((trc.grupo < 2) or (trc.saldo_inicial_mes + trc.debito_mes + trc.credito_mes <> 0))
order by trc.orden_estructura, trc.grupo, trc.fondo]]>
	</queryString>
