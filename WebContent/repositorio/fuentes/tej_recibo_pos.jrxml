	<subDataset name="detalleLiquidacionOrden" uuid="84d76df5-932c-44bb-8789-4e69ab0f19fa">
		<parameter name="TEP_NUMERO" class="java.lang.String" isForPrompting="false"/>
		<parameter name="TEP_ORGANIZACION" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
	dlo.referencia,
	substr(ref.nombre_referencia,1,40) as nombre_referencia,
	dlo.cantidad,
	dlo.precio,
	dlo.sub_total
from detalle_liquidacion_orden dlo
inner join referencia ref
on ref.referencia = dlo.referencia
where (dlo.documento, dlo.organizacion, dlo.liquidacion) in (
select drc.documento, drc.organizacion, drc.numero_credito
from detalle_recibo_cartera drc
where drc.numero = $P{TEP_NUMERO}
and drc.organizacion_parte = $P{TEP_ORGANIZACION}
and exists (select 'x' from documento_Cartera dc
	where dc.documento = drc.documento
	and  dc.organizacion = drc.organizacion
	and dc.tipo_documento = 'F')
)]]>
		</queryString>
		<field name="REFERENCIA" class="java.math.BigDecimal"/>
		<field name="NOMBRE_REFERENCIA" class="java.lang.String"/>
		<field name="CANTIDAD" class="java.math.BigDecimal"/>
		<field name="PRECIO" class="java.math.BigDecimal"/>
		<field name="SUB_TOTAL" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="detallePagoRecibo" uuid="8b4267b9-02e8-4079-9225-149e0c5b4700">
		<parameter name="TEP_NUMERO" class="java.lang.String" isForPrompting="false"/>
		<parameter name="TEP_ORGANIZACION" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
	DPR.ORGANIZACION AS ORGANIZACION,
	DPR.NUMERO AS NUMERO,
	DPR.MODALIDAD AS MODALIDAD,
	MP.NOMBRE_MODALIDAD AS DESC_MODALIDAD,
	DPR.VALOR AS VALOR_PAGADO,
	DPR.VALOR_MONEDA_BASE AS VALOR_MONEDA_BASE,
	DPR.NUMERO_DOCUMENTO AS NUMERO_DOC_SOPORTE
FROM DETALLE_PAGO_RECIBO DPR
INNER JOIN MODALIDAD_PAGO MP
ON MP.MODALIDAD = DPR.MODALIDAD
WHERE DPR.ORGANIZACION = $P{TEP_ORGANIZACION}
AND DPR.NUMERO = $P{TEP_NUMERO}]]>
		</queryString>
