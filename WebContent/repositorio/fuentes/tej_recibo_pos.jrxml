	<subDataset name="detallePagoNotaCredito" uuid="8b4267b9-02e8-4079-9225-149e0c5b4700">
		<parameter name="TEP_NUMERO" class="java.lang.String" isForPrompting="false"/>
		<parameter name="TEP_ORGANIZACION" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
             NCR.CAUSA_NOTA             AS CODIGO_CAUSA,
             CNT.NOMBRE_CAUSA_NOTA      AS DESCRIPCION,
             NCR.NOTA_CREDITO           AS NUMERO,
             ABS(DRC.VALOR)             AS VALOR

            FROM
                    DETALLE_RECIBO_CARTERA DRC,
                    NOTA_CREDITO NCR,
                    CAUSA_NOTA CNT
            WHERE
                DRC.NUMERO = $P{TEP_NUMERO}
            AND DRC.ORGANIZACION_PARTE = $P{TEP_ORGANIZACION}
            AND DRC.ORGANIZACION = NCR.ORGANIZACION
            AND DRC.NUMERO_CREDITO = NCR.NOTA_CREDITO
            AND DRC.DOCUMENTO = 'NCR'
            AND NCR.CAUSA_NOTA = CNT.CAUSA_NOTA

            ORDER BY  ABS(DRC.VALOR)]]>
		</queryString>
		<field name="CODIGO_CAUSA" class="java.lang.String"/>
		<field name="DESCRIPCION" class="java.lang.String"/>
		<field name="NUMERO" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="detalleReciboCartera" uuid="4903dc49-a72d-41ed-a582-5ea2e4d65db0">
		<parameter name="TEP_NUMERO" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
		</parameter>
		<parameter name="TEP_ORGANIZACION" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	DRC.NUMERO AS NUMERO,
	DRC.ORGANIZACION_PARTE AS ORGANIZACION_RECIBO,
	DRC.DOCUMENTO AS DOCUMENTO,
	DRC.ORGANIZACION AS ORGANIZACION,
	DRC.NUMERO_CREDITO AS NUMERO_CREDITO,
	DRC.VALOR AS VALOR,
	DRC.VALOR_NETO AS VALOR_NETO,
	DRC.ESTADO AS ESTADO,
	NVL(NVL(LO.VALOR_TOTAL, NCR.VALOR_TOTAL), NDB.VALOR_TOTAL) AS VALOR_DOCUMENTO
FROM DETALLE_RECIBO_CARTERA DRC
LEFT OUTER JOIN LIQUIDACION_ORDEN LO
ON LO.DOCUMENTO = DRC.DOCUMENTO
AND LO.ORGANIZACION = DRC.ORGANIZACION
AND LO.LIQUIDACION = DRC.NUMERO_CREDITO
LEFT OUTER JOIN NOTA_CREDITO NCR
ON DRC.DOCUMENTO = 'NCR'
AND NCR.ORGANIZACION = DRC.ORGANIZACION
AND NCR.NOTA_CREDITO = DRC.NUMERO_CREDITO
LEFT OUTER JOIN NOTA_DEBITO NDB
ON DRC.DOCUMENTO = 'NDB'
AND NDB.ORGANIZACION = DRC.ORGANIZACION
AND NDB.NOTA_DEBITO = DRC.NUMERO_CREDITO
WHERE DRC.ORGANIZACION_PARTE = $P{TEP_ORGANIZACION}
AND DRC.NUMERO = $P{TEP_NUMERO}]]>
		</queryString>
		<field name="NUMERO" class="java.math.BigDecimal"/>
		<field name="ORGANIZACION_RECIBO" class="java.lang.String"/>
		<field name="DOCUMENTO" class="java.lang.String"/>
		<field name="ORGANIZACION" class="java.lang.String"/>
		<field name="NUMERO_CREDITO" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
		<field name="VALOR_NETO" class="java.math.BigDecimal"/>
		<field name="ESTADO" class="java.lang.String"/>
		<field name="VALOR_DOCUMENTO" class="java.math.BigDecimal"/>
	</subDataset>
