	<queryString>
		<![CDATA[select
	rc.organizacion as organizacion_recibo,
	rc.numero as numero_recibo,
	rc.cliente as cliente,
	pers.identificacion as identificacion,
	trim(pers.nombre_razon_social||' '||
	pers.primer_apellido||' '||pers.segundo_apellido) as nombres,
	pers.telefono_residencia as telefono_residencia,
	pers.direccion_residencia as direccion_residencia,
	rc.periodo as periodo,
	rc.valor as valor_neto_recibo,
          rc.fecha as fecha_recibo,
    doc.documento||' - '||doc.numero_credito as numero_factura,
	(select sum(drc.valor) from detalle_recibo_cartera drc
		where drc.organizacion_parte = rc.organizacion
                     and drc.numero = rc.numero
	) as valor_documentos,
	(select sum(dpr.valor)
		from detalle_pago_recibo dpr
		where dpr.organizacion = rc.organizacion
		and dpr.numero = rc.numero
	) as valor_pagos,
          (select
            sum(drc.valor)
           from
            detalle_recibo_cartera      drc,
            recibo_caja rc
           where
            drc.organizacion = rc.organizacion
            and drc.numero = rc.numero
            and rc.numero = $P{TEP_NUMERO}
            and rc.organizacion = $P{TEP_ORGANIZACION}
            and drc.documento= 'NCR') as valor_pago_ncr,
	nvl((SELECT SUM(dlo.sub_total)
		from
		detalle_recibo_cartera drc
		inner join liquidacion_orden lo
		on lo.documento = drc.documento
		and lo.organizacion = drc.organizacion
		and lo.liquidacion = drc.numero_credito
		inner join detalle_liquidacion_orden dlo
		on dlo.documento = lo.documento
		and dlo.organizacion = lo.organizacion
		and dlo.liquidacion = lo.liquidacion
		where drc.numero = rc.numero
		and drc.organizacion_parte = rc.organizacion
		),0) as subTotal,
	nvl((SELECT SUM(dlo.sub_descuento)
		from
		detalle_recibo_cartera drc
		inner join liquidacion_orden lo
		on lo.documento = drc.documento
		and lo.organizacion = drc.organizacion
		and lo.liquidacion = drc.numero_credito
		inner join detalle_liquidacion_orden dlo
		on dlo.documento = lo.documento
		and dlo.organizacion = lo.organizacion
		and dlo.liquidacion = lo.liquidacion
		where drc.numero = rc.numero
		and drc.organizacion_parte = rc.organizacion
		),0) as subDescuento,
		nvl((SELECT SUM(dlo.sub_bruto)
		from
		detalle_recibo_cartera drc
		inner join liquidacion_orden lo
		on lo.documento = drc.documento
		and lo.organizacion = drc.organizacion
		and lo.liquidacion = drc.numero_credito
		inner join detalle_liquidacion_orden dlo
		on dlo.documento = lo.documento
		and dlo.organizacion = lo.organizacion
		and dlo.liquidacion = lo.liquidacion
		where drc.numero = rc.numero
		and drc.organizacion_parte = rc.organizacion
		),0) as subBruto,
 		nvl((SELECT SUM(dlo.sub_iva)
		from
		detalle_recibo_cartera drc
		inner join liquidacion_orden lo
		on lo.documento = drc.documento
		and lo.organizacion = drc.organizacion
		and lo.liquidacion = drc.numero_credito
		inner join detalle_liquidacion_orden dlo
		on dlo.documento = lo.documento
		and dlo.organizacion = lo.organizacion
		and dlo.liquidacion = lo.liquidacion
		where drc.numero = rc.numero
		and drc.organizacion_parte = rc.organizacion
		),0) as monto_iva,
		dc.resolucion as resolucion
from
(select drc.documento,drc.numero_credito,drc.numero,drc.organizacion
		from detalle_recibo_cartera drc
		where drc.documento IN ( $P{TEP_DOCUMENTO} )) doc

INNER JOIN recibo_caja RC
ON rc.numero = doc.numero
AND  rc.organizacion = doc.organizacion
INNER JOIN documento_cartera dc
ON  dc.documento = doc.documento
INNER JOIN cliente cli
ON cli.cliente = rc.cliente
INNER JOIN persona pers
ON pers.secuencia_persona = cli.secuencia_persona
WHERE RC.numero = $P{TEP_NUMERO}
and RC.organizacion = $P{TEP_ORGANIZACION}]]>
	</queryString>
