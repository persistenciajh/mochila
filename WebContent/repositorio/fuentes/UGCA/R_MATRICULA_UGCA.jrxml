	<queryString><![CDATA[SELECT
(SELECT MENSAJE FROM MENSAJE WHERE TIPO_CLIENTE=1 AND ROWNUM=1) texto1,
	TO_CHAR(SYSDATE,'DD/MM/YYYY')  fecha_expedicion,
	T2.tipo_identificacion			L_PERS_TIPO_IDENTIFICACION,
	to_char(DRC.orden)                            LIQUIDACION1,
-- Febrero 26 2015 - Maria Aydee
-- Se elimina la validación del Centro de Idiomas, todos si tienen saldos no genere el recibo
--	DECODE(T0.documento, 'MCI',to_char(RC.recibo_consignacion),
--		DECODE(SALDO.SALDO, 1, '000', to_char(RC.recibo_consignacion))
--	)		REFERENCIA1,
	DECODE(SALDO.SALDO, 1, '000', to_char(RC.recibo_consignacion)) REFERENCIA1,
        rc.recibo_consignacion||sd_Digito_modulo_11(rc.recibo_consignacion)  referencia_dig_ver,
                 '0' 
	 || LTRIM(
		TO_CHAR (
			RC.recibo_consignacion,
			'0000000'
		)
	) || LTRIM(
		TO_CHAR (
			rc.cliente,
			'0000000000'
		)
	) ||replace(rc.periodo,'-','0')  REFERENCIA_COMPUESTA,
                     
	RC.fecha,
	DRC.documento_orden||' '||DRC.orden orden,
	RC.cliente			CLIENTE1,
	SUBSTR(
 	                sd_nivel_centro_costo (
 		        RC.centro_costo, 
		        RC.organizacion, 
		        5
	               ),
                1,50
                )
						DEPARTAMENTO,
	SUBSTR(
                                sd_nivel_centro_costo (
 		RC.centro_costo, 
		RC.organizacion, 
		5
	                ),
                12
                )
						FACULTAD,

	sd_nivel_centro_costo (
 		RC.centro_costo, 
		RC.organizacion, 
		sd_centro_costo_estructura.nivel (
 			RC.organizacion, 
			RC.centro_costo 
		) 
	)					PROGRAMA,
	RC.periodo				PERIODO,
	T2.PAIS					L_POBL_PAIS,
	T2.DEPARTAMENTO				L_POBL_DEPARTAMENTO,
	T2.POBLACION				L_POBL_POBLACION,
	T2.identificacion				L_PERS_IDENTIFICACION,
	T1.nombre_negocio				L_CLIENTE_NOMBRE_NEGOCIO,
	T2.secuencia_persona			L_PERS_SECUENCIA_PERSONA,
	T2.nombre_razon_social||' '||T2.primer_apellido||' '||T2.segundo_apellido nombres,
	NVL(T1.direccion,' ')				L_CLIENTE_DIRECCION,
	NVL(T1.telefono,' ')				L_CLIENTE_TELEFONO,
	to_char(T1.cliente)					L_CLIENTE_CLIENTE,
	T1.secuencia_persona			L_CLIENTE_SECUENCIA_PERSONA,
	'ARMENIA'			L_POBL_NOMBRE_POBLACION,
	' '			CONDICION_COMERCIAL1,
	RC.centro_costo				CENTRO_COSTO1,
	DRC.documento_orden				DOCUMENTO1,
	DRC.organizacion_orden				ORGANIZACION1,
	NVL(RC.fecha_pago, RC.fecha) fecha,
	T2.identificacion,
	  v_semestre.periodo                                                                      periodo_estudiante,
      MEN.mensaje      mensaje    
FROM
	impresion_periodo			T,
	orden				T0,
	recibo_consignacion		RC,
	detalle_recibo_consignacion		DRC,
	cliente				T1,
	persona				T2,
	poblacion				T3,
                documento           docu,
                 v_estudiantes_registro v_semestre,
               mensaje   men,
              cliente      cli,
	(
        SELECT DISTINCT secuencia_persona, saldo
        FROM
            (
            SELECT secuencia_persona, SIGN(SUM(VE.SALDO)) saldo
            FROM v_est_cuenta_saldos_cliente VE
            GROUP BY VE.secuencia_persona
            HAVING SIGN(SUM(VE.SALDO)) = 1
            UNION ALL
            SELECT secuencia_persona, SIGN(SUM(O.valor_total)) saldo
            FROM  orden O, cliente C
            WHERE	O.estado = 'V'
            AND O.documento IN ('INTS','EC','SUP','VAL','DGPR','HAB','CNIV','DGPS','MCI','MPR')
            AND (O.documento, O.orden) NOT IN ( select  $P{CCP_DOCUMENTO}, $P{CCP_INICIAL} from dual )
            AND	NOT EXISTS (
                  SELECT null
                  FROM	liquidacion_orden L
                  WHERE	L.organizacion = O.organizacion
                  AND	L.documento = O.documento
                  AND	L.orden = O.orden )
            AND C.cliente = O.cliente_solicitado
            GROUP BY secuencia_persona
            HAVING SIGN(SUM(O.valor_total)) = 1
            )
	)saldo
WHERE
T0.estado = 'V'
AND	T0.documento = NVL(UPPER($P{CCP_DOCUMENTO}), T0.DOCUMENTO)
AND	T0.organizacion = $P{CCP_ORGANIZACION}
AND        T0.periodo=nvl($P{CCP_PERIODO},T0.PERIODO)
AND	T0.orden =NVL($P{CCP_INICIAL},T0.orden) 
AND	T.periodo(+) = T0.periodo
AND	T.cliente(+) = T0.cliente_solicitado
AND	DRC.documento_orden = T0.documento
AND	DRC.organizacion_orden = T0.organizacion
AND	DRC.orden = T0.orden
AND	RC.estado = 'I'
AND	RC.recibo_consignacion = DRC.recibo_consignacion
AND	RC.recibo_consignacion  IN (
                SELECT   MAX (recibo.recibo_consignacion)
                FROM recibo_consignacion recibo
                WHERE recibo.cliente = T0.cliente_solicitado
                 AND recibo.periodo = T0.periodo
                 AND recibo.centro_costo = T0.centro_costo
                 AND recibo.estado  = 'I' --Impreso 
             )
AND	T0.cliente_solicitado = T1.cliente
AND	T2.secuencia_persona = T1.secuencia_persona
AND	T3.departamento (+)= T2.departamento 
AND	T3.pais (+)= T2.pais
AND	T3.poblacion (+)= T2.poblacion
AND        T0.DOCUMENTO= docu.DOCUMENTO
and         T0.ORGANIZACION = docu.ORGANIZACION
AND        V_SEMESTRE.cliente (+) = t0.cliente_solicitado
AND RC.CLIENTE = CLI.CLIENTE
AND CLI.TIPO_CLIENTE = MEN.TIPO_CLIENTE(+)
--AND ROWNUM = (:P_final - :P_inicial) + 1
AND saldo.secuencia_persona (+)= T1.secuencia_persona 
ORDER BY RC.recibo_consignacion DESC]]></queryString>

