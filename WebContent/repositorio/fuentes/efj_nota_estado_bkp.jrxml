	</parameter>
	<parameter name="EFP_SEC_NOTA_PERIODO" class="java.lang.String">
		<parameterDescription><![CDATA[Nota Contable]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH TOTAL_GRUPO_CONTABLE AS (
        SELECT
            SDG.SEC_Grupo_Contable,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Debito, 0)) as Debito_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Credito, 0)) as Credito_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Debito, 0)) as Debito_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Credito, 0)) as Credito_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Act
        FROM COV_SALDO_DETALLADO_GRUPO SDG
        WHERE ((SDG.ANO = $P{EFP_ANO_BASE} AND SDG.MES = $P{EFP_MES_BASE}) OR
               (SDG.ANO = $P{EFP_ANO_BASE} - 1 AND SDG.MES = $P{EFP_MES_BASE})
              )
        GROUP BY
            SDG.SEC_Grupo_Contable
    ),
    DETALLE_RENGLON AS (
        select
            ren.orden_estructura,
            ren.nivel,
            ren.sec_renglon,
            ren.mostrar_detalle,
            sdr.codigo_contable,
            sdr.unidad,
            sdr.fondo,
            sdr.fuente_funcion,
            sdr.secuencia_persona,
            sum(sdr.saldo_inicial) as saldo_inicial,
            sum(sdr.debito) as debito,
            sum(sdr.credito) as credito,
            sum(sdr.saldo_inicial + sdr.debito - sdr.credito) as saldo_final
        from efv_renglon ren
            inner join efv_saldo_detallado_renglon sdr
                on sdr.sec_renglon = ren.sec_renglon
                    and sdr.ano = $P{EFP_ANO_BASE}
                        and sdr.mes = $P{EFP_MES_BASE}
        where ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
        group by
            ren.orden_estructura,
            ren.nivel,
            ren.sec_renglon,
            ren.mostrar_detalle,
            sdr.codigo_contable,
            sdr.unidad,
            sdr.fondo,
            sdr.fuente_funcion,
            sdr.secuencia_persona
    )
select
  rst.grupo as aux_grupo,
  rst.orden as aux_orden,
  efi.sec_estado_financiero as sec_estado_financiero,
  (case rst.grupo when 1 then efi.estado_financiero else null end) as estado_financiero,
  efi.nombre as nombre_estado_financiero,
  rst.sec_renglon as sec_renglon,
  (case rst.grupo when 1 then ren.renglon else null end) as renglon,
  (case rst.grupo when 1 then ren.detalla_anexo else null end) as detalla_anexo,
  rst.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
  (case rst.grupo when 1 then npr.nota_periodo when 2 then npr.nota_periodo else null end) as nota_periodo,
  (case rst.grupo when 2 then npr.descripcion else null end) as descripcion_nota,
  rst.codigo_nivel_3 as codigo_nivel_3,
  rst.nombre_nivel_3 as nombre_nivel_3,
  rst.codigo_nivel_4 as codigo_nivel_4,
  rst.nombre_nivel_4 as nombre_nivel_4,
  to_number(decode(rst.grupo, 2, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.saldo_inicial/1000, 'M', rst.saldo_inicial/1000000, rst.saldo_inicial))) as saldo_inicial,
  to_number(decode(rst.grupo, 2, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.debito/1000, 'M', rst.debito/1000000, rst.debito))) as debito,
  to_number(decode(rst.grupo, 2, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.credito/1000, 'M', rst.credito/1000000, rst.credito))) as credito,
  to_number(decode(rst.grupo, 2, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.saldo_final/1000, 'M', rst.saldo_final/1000000, rst.saldo_final))) as saldo_final,
  to_number(decode(rst.grupo, 2, null, decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.valor/1000, 'M', rst.valor/1000000, rst.valor))) as valor,
  eim.negrilla as aux_negrilla,
  eim.italica as aux_italica,
  eim.subrayado as aux_subrayado,
  eim.tamano_caracter as aux_tamano_caracter
from(
    SELECT -- RENGLON
        1 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        NULL AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        NULL AS CODIGO_NIVEL_4,
        NULL AS NOMBRE_NIVEL_4,
        NULL AS SALDO_INICIAL,
        NULL AS DEBITO,
        NULL AS CREDITO,
        NULL AS SALDO_FINAL,
        NVL(TRA.TOTAL_RENGLON, 0) AS VALOR
    FROM EFV_RENGLON REN
        LEFT OUTER JOIN (
            SELECT
                REN.SEC_RENGLON,
                SUM(TRE.TOTAL_RENGLON) AS TOTAL_RENGLON
            FROM EFV_RENGLON REN
                INNER JOIN EFT_RENGLON_J JER
                    ON JER.SEC_RENGLON = REN.SEC_RENGLON
                LEFT OUTER JOIN (--OJO: PUEDE SER LEFT OUTER
                    SELECT
                        RGV.SEC_RENGLON,
                        SUM(DECODE(RGV.CAMBIO_SIGNO, 'S', RGV.TOTAL_GRUPO * -1, RGV.TOTAL_GRUPO)) AS TOTAL_RENGLON
                    FROM (
                        SELECT
                            GCR.SEC_RENGLON,
                            TGC.SEC_GRUPO_CONTABLE,
                            GCO.CAMBIO_SIGNO,
                            SUM(DECODE(GCR.TIPO_VALOR,
                                        'SC', TGC.SALDO_FINAL_ACT,
                                        'VR', TGC.SALDO_FINAL_ACT - TGC.SALDO_FINAL_ANT,
                                        'MD', TGC.DEBITO_ACT,
                                        'MC', TGC.CREDITO_ACT,
                                        'VA', TGC.DEBITO_ACT - TGC.CREDITO_ACT,
                                        'VP', TGC.CREDITO_ACT - TGC.DEBITO_ACT,
                                        'SA', TGC.SALDO_FINAL_ANT
                                       )
                               ) AS TOTAL_GRUPO
                        FROM TOTAL_GRUPO_CONTABLE TGC
                            INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
                                ON GCR.SEC_GRUPO_CONTABLE = TGC.SEC_GRUPO_CONTABLE
                            INNER JOIN COT_GRUPO_CONTABLE GCO
                                ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
                        GROUP BY
                            GCR.SEC_RENGLON,
                            TGC.SEC_GRUPO_CONTABLE,
                            GCO.CAMBIO_SIGNO
                        ) RGV
                    GROUP BY RGV.SEC_RENGLON
                ) TRE
                    ON TRE.SEC_RENGLON = JER.SEC_RENGLON
            GROUP BY REN.SEC_RENGLON
        ) TRA
            ON TRA.SEC_RENGLON = REN.SEC_RENGLON
        LEFT OUTER JOIN EFT_NOTA_PERIODO_RENGLON NPR
            ON NPR.SEC_RENGLON = REN.SEC_RENGLON
    WHERE REN.SEC_ESTADO_FINANCIERO = $P{EFP_ESTADO_FINANCIERO}
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}

    UNION ALL

    SELECT --NOTA
        2 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        NULL AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        NULL AS CODIGO_NIVEL_4,
        NULL AS NOMBRE_NIVEL_4,
        NULL AS SALDO_INICIAL,
        NULL AS DEBITO,
        NULL AS CREDITO,
        NULL AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
    WHERE NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
        AND NPR.ANO = $P{EFP_ANO_BASE}
            AND NPR.MES = $P{EFP_MES_BASE}

    UNION ALL

    SELECT -- RENGLON - CUENTA
        3 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.CODIGO_CONTABLE) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE AS CODIGO_NIVEL_3,
        PCO.NOMBRE_CUENTA AS NOMBRE_NIVEL_3,
        NULL AS CODIGO_NIVEL_4,
        NULL AS NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN PLAN_CONTABLE PCO
            ON PCO.CODIGO_CONTABLE = DRE.CODIGO_CONTABLE
    WHERE ((REN.DETALLA_ANEXO <> 'UNI') and (REN.DETALLA_ANEXO <> 'UGC'))
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE,
        PCO.NOMBRE_CUENTA
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0

    UNION ALL

    SELECT -- RENGLON - UNIDAD
        3 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.UNIDAD) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.UNIDAD AS CODIGO_NIVEL_3,
        CCO.NOMBRE_CENTRO_COSTO AS NOMBRE_NIVEL_3,
        NULL AS CODIGO_NIVEL_4,
        NULL AS NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN CENTRO_COSTO CCO
            ON CCO.CENTRO_COSTO = DRE.UNIDAD
    WHERE ((REN.DETALLA_ANEXO = 'UNI') and (REN.DETALLA_ANEXO = 'UGC'))
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.UNIDAD,
        CCO.NOMBRE_CENTRO_COSTO
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0

    UNION ALL

    SELECT -- RENGLON - CUENTA - FONDO
        4 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.CODIGO_CONTABLE || DRE.FONDO) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        NVL(TO_CHAR(DRE.FONDO), 'SFO') AS CODIGO_NIVEL_4,
        NVL(FON.NOMBRE_FONDO, '-=|SIN FONDO|=-') AS NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        LEFT OUTER JOIN FONDO FON
            ON FON.FONDO = DRE.FONDO
    WHERE REN.DETALLA_ANEXO = 'CFO'
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE,
        DRE.FONDO,
        FON.NOMBRE_FONDO
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0

    UNION ALL

    SELECT -- RENGLON - CUENTA - FUENTE FUNCION
        4 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.CODIGO_CONTABLE || DRE.FUENTE_FUNCION) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        NVL(TO_CHAR(DRE.FUENTE_FUNCION), 'SFF') AS CODIGO_NIVEL_4,
        NVL(FFU.NOMBRE_FUENTE_FUNCION, '-=|SIN FUENTE FUNCION|=-') AS NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        LEFT OUTER JOIN FUENTE_FUNCION FFU
            ON FFU.FUENTE_FUNCION = DRE.FUENTE_FUNCION
    WHERE REN.DETALLA_ANEXO = 'CFF'
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE,
        DRE.FUENTE_FUNCION,
        FFU.NOMBRE_FUENTE_FUNCION
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0

    UNION ALL

    SELECT -- RENGLON - CUENTA - TERCERO
        4 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.CODIGO_CONTABLE || PER.IDENTIFICACION) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        NVL(TO_CHAR(PER.IDENTIFICACION), 'STE') AS CODIGO_NIVEL_4,
        NVL((TRIM(PER.NOMBRE_RAZON_SOCIAL || ' ' || NVL(PER.PRIMER_APELLIDO,'') || ' ' || NVL(PER.SEGUNDO_APELLIDO,''))), 'SIN TERCERO') as NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        LEFT OUTER JOIN PERSONA PER
            ON PER.SECUENCIA_PERSONA = DRE.SECUENCIA_PERSONA
    WHERE REN.DETALLA_ANEXO = 'CTE'
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE,
        PER.IDENTIFICACION,
        PER.NOMBRE_RAZON_SOCIAL,
        PER.PRIMER_APELLIDO,
        PER.SEGUNDO_APELLIDO
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0

    UNION ALL

    SELECT -- RENGLON - CUENTA - UNIDAD
        4 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.CODIGO_CONTABLE || DRE.UNIDAD) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        NVL(TO_CHAR(DRE.UNIDAD), 'SUN') AS CODIGO_NIVEL_4,
        NVL(CCO.NOMBRE_CENTRO_COSTO, '-=|SIN UNIDAD|=-') AS NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        LEFT OUTER JOIN CENTRO_COSTO CCO
            ON CCO.CENTRO_COSTO = DRE.UNIDAD
    WHERE REN.DETALLA_ANEXO = 'CUN'
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.CODIGO_CONTABLE,
        DRE.UNIDAD,
        CCO.NOMBRE_CENTRO_COSTO
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0

    UNION ALL

    SELECT -- RENGLON - UNIDAD - GRUPO CONTABLE
        4 AS GRUPO,
        TO_CHAR(NPR.SEC_RENGLON || NPR.SEC_NOTA_PERIODO_RENGLON || DRE.UNIDAD) AS ORDEN,
        NPR.SEC_RENGLON AS SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON AS SEC_NOTA_PERIODO_RENGLON,
        DRE.UNIDAD AS CODIGO_NIVEL_3,
        NULL AS NOMBRE_NIVEL_3,
        TO_CHAR(GCR.SEC_GRUPO_CONTABLE) AS CODIGO_NIVEL_4,
        GCO.NOMBRE AS NOMBRE_NIVEL_4,
        SUM(DRE.SALDO_INICIAL) AS SALDO_INICIAL,
        SUM(DRE.DEBITO) AS DEBITO,
        SUM(DRE.CREDITO) AS CREDITO,
        SUM(DRE.SALDO_FINAL) AS SALDO_FINAL,
        NULL AS VALOR
    FROM EFT_NOTA_PERIODO_RENGLON NPR
        INNER JOIN EFV_RENGLON REN
            ON REN.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN DETALLE_RENGLON DRE
            ON DRE.SEC_RENGLON = NPR.SEC_RENGLON
        INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
            ON GCR.SEC_RENGLON = DRE.SEC_RENGLON
        INNER JOIN COT_GRUPO_CONTABLE GCO
            ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
        INNER JOIN COT_CUENTA_GRUPO_CONTABLE CGC
            ON CGC.SEC_GRUPO_CONTABLE = GCO.SEC_GRUPO_CONTABLE
                AND CGC.CODIGO_CONTABLE = DRE.CODIGO_CONTABLE
    WHERE REN.DETALLA_ANEXO = 'UGC'
        AND NPR.SEC_NOTA_PERIODO_RENGLON LIKE NVL(TO_CHAR($P{EFP_SEC_NOTA_PERIODO}),'%')
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    GROUP BY
        NPR.SEC_RENGLON,
        NPR.SEC_NOTA_PERIODO_RENGLON,
        DRE.UNIDAD,
        GCR.SEC_GRUPO_CONTABLE,
        GCO.NOMBRE
    HAVING (SUM(DRE.SALDO_INICIAL) + SUM(DRE.DEBITO) + SUM(DRE.CREDITO)) <> 0
    ) rst
  inner join eft_renglon ren
    on ren.sec_renglon = rst.sec_renglon
  inner join eft_estado_financiero efi
    on efi.sec_estado_financiero = ren.sec_estado_financiero
  inner join eft_nota_periodo_renglon npr
    on npr.sec_nota_periodo_renglon = rst.sec_nota_periodo_renglon
  left outer join eft_efecto_impresion eim
    on eim.sec_efecto_impresion = ren.sec_efecto_impresion
where efi.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
    and ((rst.grupo < 3) or (rst.saldo_inicial + rst.debito + rst.credito <> 0))
order by rst.orden, rst.grupo, rst.codigo_nivel_3, rst.codigo_nivel_4]]>
	</queryString>
