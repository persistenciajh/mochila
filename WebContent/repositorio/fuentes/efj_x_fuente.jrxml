	<queryString>
		<![CDATA[WITH TOTAL_GRUPO_CONTABLE AS
  (SELECT SDG.SEC_Grupo_Contable,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Saldo_Inicial, 0)) AS Saldo_Inicial_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Debito, 0)) AS Debito_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Credito, 0)) AS Credito_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}   - 1
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) AS Saldo_Final_Ant,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Saldo_Inicial, 0)) AS Saldo_Inicial_Act,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Debito, 0)) AS Debito_Act,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Credito, 0)) AS Credito_Act,
    SUM(DECODE(TO_CHAR(SDG.Ano
    || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE}
    || TO_NUMBER($P{EFP_MES_BASE})), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) AS Saldo_Final_Act
  FROM COV_SALDO_DETALLADO_GRUPO SDG
  WHERE ((SDG.ANO = $P{EFP_ANO_BASE}
  AND SDG.MES     = TO_NUMBER($P{EFP_MES_BASE}))
  OR (SDG.ANO     = $P{EFP_ANO_BASE} - 1
  AND SDG.MES     = TO_NUMBER($P{EFP_MES_BASE})) )
  GROUP BY SDG.SEC_Grupo_Contable
  ),
  DETALLE_FUENTE_FUNCION AS
  (SELECT
    ren.orden_estructura,
    ren.sec_estado_financiero,
    sdr.mes,
    sdr.ano,
    ren.nivel,
    ren.sec_renglon,
    ren.mostrar_detalle,
    sdr.fuente_funcion,
    SDR.CODIGO_CONTABLE,
    SUM(sdr.saldo_inicial)                            AS saldo_inicial,
    SUM(sdr.debito)                                   AS debito,
    SUM(sdr.credito)                                  AS credito,
    SUM(sdr.saldo_inicial + sdr.debito - sdr.credito) AS saldo_final
  FROM efv_renglon ren
  INNER JOIN efv_saldo_detallado_renglon sdr
  ON sdr.sec_renglon              = ren.sec_renglon
  AND sdr.ano||sdr.mes            = $P{EFP_ANO_BASE}||TO_NUMBER($P{EFP_MES_BASE})
  WHERE ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
  GROUP BY ren.orden_estructura,
    ren.nivel,
    ren.sec_renglon,
    ren.mostrar_detalle,
    SDR.CODIGO_CONTABLE,
    sdr.mes,
    sdr.ano,
    ren.sec_estado_financiero,
    sdr.fuente_funcion
  )
SELECT
  1                            AS grupo,
  ren.orden_estructura         AS orden_estructura,
  ren.sec_estado_financiero    AS sec_estado_financiero,
  efi.estado_financiero        AS estado_financiero,
  efi.nombre                   AS nombre_estado_financiero,
  REN.sec_renglon              AS sec_renglon,
  ren.renglon                  AS renglon,
  ren.separador                AS separador,
  ren.origen_valor             AS origen_valor,
  TRA.tipo_valor               AS tipo_saldo,
  ren.mostrar_saldo            AS mostrar_saldo,
  TRA.cambio_signo             AS cambio_signo,
  ren.nivel                    AS aux_nivel,
  npr.nota_periodo             AS nota_periodo,
  NULL                         AS fuente_funcion,
  NULL                         AS nombre_fuente_funcion,
  NULL                         AS saldo_inicial_base,
  NULL                         AS debito_base,
  NULL                         AS credito_base,
  NVL(DECODE($P{GEP_PRESENTACION_CIFRAS},'K', TRA.TOTAL_RENGLON/1000,'M', TRA.TOTAL_RENGLON/1000000,TRA.TOTAL_RENGLON), 0) AS saldo_final_base,
  eim.negrilla        AS aux_negrilla,
  eim.italica         AS aux_italica,
  eim.subrayado       AS aux_subrayado,
  eim.tamano_caracter AS aux_tamano
FROM efv_renglon ren
LEFT OUTER JOIN
  (SELECT REN.SEC_RENGLON,
    TRE.TIPO_VALOR,
    TRE.CAMBIO_SIGNO,
    SUM(TRE.TOTAL_RENGLON) AS TOTAL_RENGLON
  FROM EFV_RENGLON REN
  INNER JOIN EFT_RENGLON_J JER
  ON JER.SEC_RENGLON = REN.SEC_RENGLON
  LEFT OUTER JOIN
    (--OJO PUEDE SER LEFT OUTER
    SELECT RGV.SEC_RENGLON,
      RGV.TIPO_VALOR,
      RGV.CAMBIO_SIGNO,
      SUM(DECODE(RGV.CAMBIO_SIGNO, 'S', RGV.TOTAL_GRUPO * -1, RGV.TOTAL_GRUPO)) AS TOTAL_RENGLON
    FROM
      (SELECT GCR.SEC_RENGLON,
        TGC.SEC_GRUPO_CONTABLE,
        GCO.CAMBIO_SIGNO,
        GCR.TIPO_VALOR,
        SUM(DECODE(GCR.TIPO_VALOR, 'SC', TGC.SALDO_FINAL_ACT, 'VR', TGC.SALDO_FINAL_ACT - TGC.SALDO_FINAL_ANT, 'MD', TGC.DEBITO_ACT, 'MC', TGC.CREDITO_ACT, 'VA', TGC.DEBITO_ACT - TGC.CREDITO_ACT, 'VP', TGC.CREDITO_ACT - TGC.DEBITO_ACT, 'SA', TGC.SALDO_FINAL_ANT ) ) AS TOTAL_GRUPO
      FROM
        TOTAL_GRUPO_CONTABLE TGC
      INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
      ON GCR.SEC_GRUPO_CONTABLE = TGC.SEC_GRUPO_CONTABLE
      INNER JOIN COT_GRUPO_CONTABLE GCO
      ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
      GROUP BY GCR.SEC_RENGLON,
        TGC.SEC_GRUPO_CONTABLE ,
        GCR.TIPO_VALOR,
        GCO.CAMBIO_SIGNO
      ) RGV
    GROUP BY RGV.SEC_RENGLON,
      RGV.TIPO_VALOR,
      RGV.CAMBIO_SIGNO
    ) TRE ON TRE.SEC_RENGLON = JER.SEC_RENGLON
  GROUP BY REN.SEC_RENGLON,
    TRE.TIPO_VALOR,
    TRE.CAMBIO_SIGNO
  ) TRA ON TRA.SEC_RENGLON = REN.SEC_RENGLON
INNER JOIN EFT_ESTADO_FINANCIERO EFI
ON EFI.SEC_ESTADO_FINANCIERO = REN.SEC_ESTADO_FINANCIERO
LEFT OUTER JOIN eft_nota_periodo_renglon npr
ON npr.sec_renglon = REN.sec_renglon
AND npr.ano        = $P{EFP_ANO_BASE}
AND npr.mes        = TO_NUMBER($P{EFP_MES_BASE})
LEFT OUTER JOIN EFT_EFECTO_IMPRESION EIM
ON EIM.SEC_EFECTO_IMPRESION     = REN.SEC_EFECTO_IMPRESION
WHERE EFI.SEC_ESTADO_FINANCIERO = $P{EFP_ESTADO_FINANCIERO}
UNION ALL
SELECT 2 AS grupo,
  ren.orden_estructura,
  ren.sec_estado_financiero    AS sec_estado_financiero,
  efi.estado_financiero        AS estado_financiero,
  efi.nombre                   AS nombre_estado_financiero,
  REN.sec_renglon              AS sec_renglon,
  ren.renglon                  AS renglon,
  ren.separador                AS separador,
  ren.origen_valor             AS origen_valor,
  NULL                         AS tipo_saldo,
  ren.mostrar_saldo            AS mostrar_saldo,
  NULL                         AS cambio_signo,
  (ren.nivel+1)                AS aux_nivel,
  NULL                         AS nota_periodo,
  NVL(TO_CHAR(DFF.fuente_funcion), 'SFF')                          AS fuente_funcion,
  NVL(ffu.nombre_fuente_funcion, '-=|SIN FUENTE FUNCION|=-')       AS nombre_fuente_funcion,
  NVL(DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', DFF.saldo_inicial/1000, 'M', DFF.saldo_inicial/1000000, DFF.saldo_inicial),0)     AS saldo_inicial_base,
  NVL(DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', DFF.debito       /1000, 'M', DFF.debito/1000000, DFF.debito),0)                   AS debito_base,
  NVL(DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', DFF.credito      /1000, 'M', DFF.credito/1000000, DFF.credito),0)                 AS credito_base,
  NVL(DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', DFF.saldo_final  /1000, 'M', DFF.saldo_final/1000000, DFF.saldo_final),0)         AS saldo_final_base,
  NULL          AS aux_negrilla,
  NULL          AS aux_italica,
  NULL          AS aux_subrayado,
  NULL          AS aux_tamano
FROM efv_renglon ren
INNER JOIN DETALLE_FUENTE_FUNCION DFF
ON DFF.SEC_RENGLON = REN.SEC_RENGLON
INNER JOIN PLAN_CONTABLE PCO
ON PCO.CODIGO_CONTABLE = DFF.CODIGO_CONTABLE
INNER JOIN eft_estado_financiero efi
ON efi.sec_estado_financiero = REN.sec_estado_financiero
LEFT OUTER JOIN fuente_funcion ffu
ON ffu.fuente_funcion                                 = DFF.fuente_funcion
LEFT OUTER JOIN EFT_EFECTO_IMPRESION EIM
ON EIM.SEC_EFECTO_IMPRESION     = REN.SEC_EFECTO_IMPRESION
WHERE (DFF.SALDO_INICIAL + DFF.DEBITO + DFF.CREDITO) <> 0
AND DFF.MOSTRAR_DETALLE                               = 'S'
ORDER BY 2,
  1,
  9]]>
	</queryString>
