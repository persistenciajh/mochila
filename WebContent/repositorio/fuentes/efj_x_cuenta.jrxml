	<queryString>
		<![CDATA[WITH TOTAL_GRUPO_CONTABLE AS (
        SELECT
            SDG.SEC_Grupo_Contable,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Debito, 0)) as Debito_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Credito, 0)) as Credito_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Debito, 0)) as Debito_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Credito, 0)) as Credito_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || SDG.Mes), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Act
        FROM COV_SALDO_DETALLADO_GRUPO SDG
        WHERE ((SDG.ANO = $P{EFP_ANO_BASE} AND SDG.MES = $P{EFP_MES_BASE}) OR
               (SDG.ANO = $P{EFP_ANO_BASE} - 1 AND SDG.MES = $P{EFP_MES_BASE})
              )
        GROUP BY
            SDG.SEC_Grupo_Contable
    ),
    DETALLE_CUENTA AS (
        select
            ren.orden_estructura,
            ren.nivel,
            ren.sec_renglon,
            ren.mostrar_detalle,
            sdr.codigo_contable,
            sum(sdr.saldo_inicial) as saldo_inicial,
            sum(sdr.debito) as debito,
            sum(sdr.credito) as credito,
            sum(sdr.saldo_inicial + sdr.debito - sdr.credito) as saldo_final
        from efv_renglon ren
            inner join efv_saldo_detallado_renglon sdr
                on sdr.sec_renglon = ren.sec_renglon
                    and sdr.ano = $P{EFP_ANO_BASE}
                        and sdr.mes = $P{EFP_MES_BASE}
        where ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
        group by ren.orden_estructura, ren.nivel, ren.sec_renglon, ren.mostrar_detalle, sdr.codigo_contable
    )
SELECT
    1 AS GRUPO,
    REN.ORDEN_ESTRUCTURA,
    REN.NIVEL,
    REN.SEC_ESTADO_FINANCIERO,
    EFI.NOMBRE,
    DECODE(REN.SEPARADOR, 'N', NPR.NOTA_PERIODO, null) as NOTA_PERIODO,
    REN.SEC_RENGLON,
    REN.RENGLON,
    NULL AS CUENTA_CONTABLE,
    REN.SEPARADOR,
    REN.ORIGEN_VALOR,
    REN.MOSTRAR_SALDO,
    REN.MOSTRAR_DETALLE,
    NULL AS SALDO_INICIAL,
    NULL AS DEBITO,
    NULL AS CREDITO,
    NULL AS SALDO_FINAL,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(TRA.TOTAL_RENGLON, 0)/1000, 'M', NVL(TRA.TOTAL_RENGLON, 0)/1000000, NVL(TRA.TOTAL_RENGLON, 0)) AS VALOR,
    REN.SEC_EFECTO_IMPRESION,
    EIM.NEGRILLA AS AUX_NEGRILLA,
    EIM.ITALICA AS AUX_ITALICA,
    EIM.SUBRAYADO AS AUX_SUBRAYADO,
    EIM.TAMANO_CARACTER AS AUX_TAMANO
FROM EFV_RENGLON REN
    LEFT OUTER JOIN (
        SELECT
            REN.SEC_RENGLON,
            SUM(TRE.TOTAL_RENGLON) AS TOTAL_RENGLON
        FROM EFV_RENGLON REN
            INNER JOIN EFT_RENGLON_J JER
                ON JER.SEC_RENGLON = REN.SEC_RENGLON
            LEFT OUTER JOIN (--OJO: PUEDE SER LEFT OUTER
                SELECT
                    RGV.SEC_RENGLON,
                    SUM(DECODE(RGV.CAMBIO_SIGNO, 'S', RGV.TOTAL_GRUPO * -1, RGV.TOTAL_GRUPO)) AS TOTAL_RENGLON
                FROM (
                    SELECT
                        GCR.SEC_RENGLON,
                        TGC.SEC_GRUPO_CONTABLE,
                        GCO.CAMBIO_SIGNO,
                        SUM(DECODE(GCR.TIPO_VALOR,
                                    'SC', TGC.SALDO_FINAL_ACT,
                                    'VR', TGC.SALDO_FINAL_ACT - TGC.SALDO_FINAL_ANT,
                                    'MD', TGC.DEBITO_ACT,
                                    'MC', TGC.CREDITO_ACT,
                                    'VA', TGC.DEBITO_ACT - TGC.CREDITO_ACT,
                                    'VP', TGC.CREDITO_ACT - TGC.DEBITO_ACT,
                                    'SA', TGC.SALDO_FINAL_ANT
                                   )
                           ) AS TOTAL_GRUPO
                    FROM TOTAL_GRUPO_CONTABLE TGC
                        INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
                            ON GCR.SEC_GRUPO_CONTABLE = TGC.SEC_GRUPO_CONTABLE
                        INNER JOIN COT_GRUPO_CONTABLE GCO
                            ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
                    GROUP BY
                        GCR.SEC_RENGLON,
                        TGC.SEC_GRUPO_CONTABLE,
                        GCO.CAMBIO_SIGNO
                    ) RGV
                GROUP BY RGV.SEC_RENGLON
            ) TRE
                ON TRE.SEC_RENGLON = JER.SEC_RENGLON
        GROUP BY REN.SEC_RENGLON
    ) TRA
        ON TRA.SEC_RENGLON = REN.SEC_RENGLON
    INNER JOIN EFT_ESTADO_FINANCIERO EFI
        ON EFI.SEC_ESTADO_FINANCIERO = REN.SEC_ESTADO_FINANCIERO
    LEFT OUTER JOIN EFT_NOTA_PERIODO_RENGLON NPR
        ON NPR.SEC_RENGLON = REN.SEC_RENGLON
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND NPR.MES = $P{EFP_MES_BASE}
    LEFT OUTER JOIN EFT_EFECTO_IMPRESION EIM
        ON EIM.SEC_EFECTO_IMPRESION = REN.SEC_EFECTO_IMPRESION
WHERE EFI.SEC_ESTADO_FINANCIERO = $P{EFP_ESTADO_FINANCIERO}

UNION ALL

SELECT
    2 AS GRUPO,
    DCU.ORDEN_ESTRUCTURA,
    DCU.NIVEL + 1 AS NIVEL,
    NULL AS SEC_ESTADO_FINANCIERO,
    NULL AS NOMBRE,
    NULL AS NOTA_PERIODO,
    NULL AS SEC_RENGLON,
    NULL AS RENGLON,
    '[' || DCU.CODIGO_CONTABLE || '] ' || PCO.NOMBRE_CUENTA AS CUENTA_CONTABLE,
    NULL AS SEPARADOR,
    NULL AS ORIGEN_VALOR,
    NULL AS MOSTRAR_SALDO,
    DCU.MOSTRAR_DETALLE AS MOSTRAR_DETALLE,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(DCU.SALDO_INICIAL, 0)/1000, 'M', NVL(DCU.SALDO_INICIAL, 0)/1000000, NVL(DCU.SALDO_INICIAL, 0)) AS SALDO_INICIAL,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(DCU.DEBITO, 0)/1000, 'M', NVL(DCU.DEBITO, 0)/1000000, NVL(DCU.DEBITO, 0)) AS DEBITO,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(DCU.CREDITO, 0)/1000, 'M', NVL(DCU.CREDITO, 0)/1000000, NVL(DCU.CREDITO, 0)) AS CREDITO,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(DCU.SALDO_FINAL, 0)/1000, 'M', NVL(DCU.SALDO_FINAL, 0)/1000000, NVL(DCU.SALDO_FINAL, 0)) AS SALDO_FINAL,
    NULL AS VALOR,
    NULL AS SEC_EFECTO_IMPRESION,
    NULL AS AUX_NEGRILLA,
    NULL AS AUX_ITALICA,
    NULL AS AUX_SUBRAYADO,
    NULL AS AUX_TAMANO
FROM DETALLE_CUENTA DCU
    INNER JOIN PLAN_CONTABLE PCO
        ON PCO.CODIGO_CONTABLE = DCU.CODIGO_CONTABLE
WHERE (DCU.SALDO_INICIAL + DCU.DEBITO + DCU.CREDITO) <> 0
    AND DCU.MOSTRAR_DETALLE = 'S'
ORDER BY 2, 1, 9]]>
	</queryString>
