	<queryString>
		<![CDATA[SELECT
    1 AS LIBRO,
    DOCUMENTO,
    NOMBRE_DOCUMENTO,
    FECHA_MOVIMIENTO,
    NUMERO_DOCUMENTO,
    CUENTA_CONTABLE,
    NOMBRE_CUENTA,
    VALOR_DEBITO,
    VALOR_CREDITO
FROM CGR_LIBRO_DIARIO
WHERE FECHA_MOVIMIENTO BETWEEN (TO_DATE('01/' || LPAD($P{CGP_MES}, 2, '0') || '/' || $P{CGP_ANO}, 'DD/MM/YYYY')) AND (LAST_DAY(TO_DATE('01/' || LPAD($P{CGP_MES}, 2, '0') || '/' || $P{CGP_ANO}, 'DD/MM/YYYY')))
ORDER BY
    DOCUMENTO,
    FECHA_MOVIMIENTO,
    NUMERO_DOCUMENTO]]>
