	<queryString>
		<![CDATA[select
  rst.grupo as aux_grupo,
  rst.orden as aux_orden,
  org.organizacion as organizacion,
  (case rst.grupo when 1 then org.nombre_organizacion else null end) as nombre_organizacion,
  efi.sec_estado_financiero as sec_estado_financiero,
  (case rst.grupo when 1 then efi.estado_financiero else null end) as estado_financiero,
  efi.nombre as nombre_estado_financiero,
  rst.sec_renglon as sec_renglon,
  (case rst.grupo when 1 then ren.renglon else null end) as renglon,
  (case rst.grupo when 1 then ren.detalla_anexo else null end) as detalla_anexo,
  rst.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
  (case rst.grupo when 1 then npr.nota_periodo when 2 then npr.nota_periodo else null end) as nota_periodo,
  (case rst.grupo when 2 then npr.descripcion else null end) as descripcion_nota,
  rst.codigo_nivel_3 as codigo_nivel_3,
  rst.nombre_nivel_3 as nombre_nivel_3,
  rst.codigo_nivel_4 as codigo_nivel_4,
  rst.nombre_nivel_4 as nombre_nivel_4,
  decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.saldo_final_base/1000, 'M', rst.saldo_final_base/1000000, rst.saldo_final_base) as saldo_final_base,
  decode((nvl(rst.saldo_final_base,0) + nvl(rst.saldo_final_base_padre,0)), 0, 0, ((rst.saldo_final_base * 100) / decode(nvl(rst.saldo_final_base_padre,0), 0, rst.saldo_final_base, rst.saldo_final_base_padre))) as analisis_vertical_base,
  decode($P{GEP_PRESENTACION_CIFRAS}, 'K', rst.saldo_final_comp/1000, 'M', rst.saldo_final_comp/1000000, rst.saldo_final_comp) as saldo_final_comp,
  decode((nvl(rst.saldo_final_comp,0) + nvl(rst.saldo_final_comp_padre,0)), 0, 0, ((rst.saldo_final_comp * 100) / decode(nvl(rst.saldo_final_comp_padre,0), 0, rst.saldo_final_comp, rst.saldo_final_comp_padre))) as analisis_vertical_comp,
  decode($P{GEP_PRESENTACION_CIFRAS}, 'K', abs((nvl(rst.saldo_final_base, 0) - nvl(rst.saldo_final_comp, 0)))/1000, 'M', abs((nvl(rst.saldo_final_base, 0) - nvl(rst.saldo_final_comp, 0)))/1000000, abs((nvl(rst.saldo_final_base, 0) - nvl(rst.saldo_final_comp, 0)))) as variacion,
   case
                                           when rst.saldo_final_base = 0  and rst.saldo_final_comp = 0 then 0
                                           when rst.saldo_final_base != 0 and rst.saldo_final_comp = 0 then 100
                                           when rst.saldo_final_base = 0  and rst.saldo_final_comp != 0 then -100
                                           else (abs(nvl(rst.saldo_final_base, 0) - nvl(rst.saldo_final_comp, 0)) * 100) / rst.saldo_final_comp
                                        end as porcentaje_variacion,
  eim.negrilla as aux_negrilla,
  eim.italica as aux_italica,
  eim.subrayado as aux_subrayado,
  eim.tamano_caracter as aux_tamano
from(
    select
      1 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      null as codigo_nivel_3,
      null as nombre_nivel_3,
      null as codigo_nivel_4,
      null as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
           from efv_total_renglon_mes
           where sec_renglon = sdr.sec_renglon
           and ano = $P{EFP_ANO_BASE}
               and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
           from efv_total_renglon_mes
           where sec_renglon = sdr.sec_renglon
           and ano = $P{EFP_ANO_COMP}
               and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
    where npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
        and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
      npr.sec_renglon,
      sdr.sec_renglon,
      npr.sec_nota_periodo_renglon
    union all
    select --Nota
      2 as grupo,
      to_char(npr.sec_renglon || npr.sec_nota_periodo_renglon) as orden,
      npr.sec_renglon as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      null as codigo_nivel_3,
      null as nombre_nivel_3,
      null as codigo_nivel_4,
      null as nombre_nivel_4,
      null as saldo_final_base,
      null as saldo_final_base_padre,
      null as saldo_final_comp,
      null as saldo_final_comp_padre
    from eft_nota_periodo_renglon npr
    where npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
        and ((npr.ano || lpad(npr.mes, 2, '0')) = ($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}))
    union all
    select --Renglon - Cuenta
      3 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.codigo_contable) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      sdr.codigo_contable as codigo_nivel_3,
      pc.nombre_cuenta as nombre_nivel_3,
      null as codigo_nivel_4,
      null as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
           from efv_total_renglon_mes
           where sec_renglon = sdr.sec_renglon
           and ano = $P{EFP_ANO_BASE}
               and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
           from efv_total_renglon_mes
           where sec_renglon = sdr.sec_renglon
           and ano = $P{EFP_ANO_COMP}
               and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
      inner join plan_contable pc
        on pc.codigo_contable = sdr.codigo_contable
    where ((sdr.detalla_anexo <> 'UNI') and (sdr.detalla_anexo <> 'UGC'))
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
      npr.sec_renglon,
      sdr.sec_renglon,
      npr.sec_nota_periodo_renglon,
      sdr.codigo_contable,
      pc.nombre_cuenta
    union all
    select -- Renglon - Unidad
        3 as grupo,
        to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.unidad) as orden,
        nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
        npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
        sdr.unidad as codigo_nivel_3,
        uni.nombre_centro_costo as nombre_nivel_3,
        null as codigo_nivel_4,
        null as nombre_nivel_4,
        nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
        nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_BASE}
                   and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
        nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
        nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_COMP}
                   and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
        inner join centro_costo uni
            on uni.centro_costo = sdr.unidad
    where ((sdr.detalla_anexo = 'UNI') or (sdr.detalla_anexo = 'UGC'))
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
        npr.sec_renglon,
        sdr.sec_renglon,
        npr.sec_nota_periodo_renglon,
        sdr.unidad,
        uni.nombre_centro_costo
    union all
    select
      4 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.codigo_contable || sdr.fondo) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      sdr.codigo_contable as codigo_nivel_3,
      null as nombre_nivel_3,
      nvl(to_char(sdr.fondo),'SFO') as codigo_nivel_4,
      nvl(fon.nombre_fondo,'-=|SIN FONDO|=-') as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_BASE}
                   and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_COMP}
                   and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
      left outer join fondo fon
        on fon.fondo = sdr.fondo
    where sdr.detalla_anexo = 'CFO'
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
      npr.sec_renglon,
      sdr.sec_renglon,
      npr.sec_nota_periodo_renglon,
      sdr.codigo_contable,
      sdr.fondo,
      fon.nombre_fondo
    union all
    select --Renglon - Cuenta - Fuente Funcion
      4 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.codigo_contable || sdr.fuente_funcion) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      sdr.codigo_contable as codigo_nivel_3,
      null as nombre_nivel_3,
      nvl(to_char(sdr.fuente_funcion),'SFF') as codigo_nivel_4,
      nvl(ffu.nombre_fuente_funcion,'-=|SIN FUENTE FUNCION|=-') as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_BASE}
                   and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_COMP}
                   and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
      left outer join fuente_funcion ffu
        on ffu.fuente_funcion = sdr.fuente_funcion
    where sdr.detalla_anexo = 'CFF'
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
      npr.sec_renglon,
      sdr.sec_renglon,
      npr.sec_nota_periodo_renglon,
      sdr.codigo_contable,
      sdr.fuente_funcion,
      ffu.nombre_fuente_funcion
    union all
    select --Renglon - Cuenta - Tercero
      4 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.codigo_contable || per.identificacion) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      sdr.codigo_contable as codigo_nivel_3,
      null as nombre_nivel_3,
      nvl(per.identificacion,'STE') as codigo_nivel_4,
      nvl((trim(per.nombre_razon_social || ' ' || nvl(per.primer_apellido,'') || ' ' || nvl(per.segundo_apellido,''))), '-=|SIN TERCERO|=-') as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_BASE}
                   and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_COMP}
                   and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
      left outer join persona per
        on per.secuencia_persona = sdr.secuencia_persona
    where sdr.detalla_anexo = 'CTE'
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
      npr.sec_renglon,
      sdr.sec_renglon,
      npr.sec_nota_periodo_renglon,
      sdr.codigo_contable,
      per.identificacion,
      per.nombre_razon_social,
      per.primer_apellido,
      per.segundo_apellido
    union all
    select
      4 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.codigo_contable || sdr.unidad) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      sdr.codigo_contable as codigo_nivel_3,
      null as nombre_nivel_3,
      nvl(sdr.unidad,'SUN') as codigo_nivel_4,
      nvl(uni.nombre_centro_costo, '-=|SIN UNIDAD|=-') as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_BASE}
                   and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_COMP}
                   and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
      left outer join centro_costo uni
        on uni.centro_costo = sdr.unidad
    where sdr.detalla_anexo = 'CUN'
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
      npr.sec_renglon,
      sdr.sec_renglon,
      npr.sec_nota_periodo_renglon,
      sdr.codigo_contable,
      sdr.unidad,
      uni.nombre_centro_costo
    union all
    select --Renglon - Unidad - Grupo contable
      4 as grupo,
      to_char(nvl(sdr.sec_renglon, npr.sec_renglon) || npr.sec_nota_periodo_renglon || sdr.unidad) as orden,
      nvl(sdr.sec_renglon, npr.sec_renglon) as sec_renglon,
      npr.sec_nota_periodo_renglon as sec_nota_periodo_renglon,
      sdr.unidad as codigo_nivel_3,
      null as nombre_nivel_3,
      to_char(gcr.sec_grupo_contable) as codigo_nivel_4,
      gc.nombre as nombre_nivel_4,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_base,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_BASE}
                   and lpad(mes, 2, '0') = $P{EFP_MES_BASE}),0) as saldo_final_base_padre,
      nvl(sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)),0) as saldo_final_comp,
      nvl((select sum(saldo_inicial + debito - credito)
               from efv_total_renglon_mes
               where sec_renglon = sdr.sec_renglon
               and ano = $P{EFP_ANO_COMP}
                   and lpad(mes, 2, '0') = $P{EFP_MES_COMP}),0) as saldo_final_comp_padre
    from efv_saldo_detallado_renglon sdr
        right outer join eft_nota_periodo_renglon npr
            on npr.sec_renglon = sdr.sec_renglon
                and npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE}
        inner join centro_costo uni
            on uni.centro_costo = sdr.unidad
        inner join eft_grupo_contable_renglon gcr
            on gcr.sec_renglon = sdr.sec_renglon
        inner join cot_grupo_contable gc
            on gc.sec_grupo_contable = gcr.sec_grupo_contable
        inner join cot_cuenta_grupo_contable cgc
            on cgc.sec_grupo_contable = gcr.sec_grupo_contable
                and cgc.codigo_contable = sdr.codigo_contable
    where sdr.detalla_anexo = 'UGC'
        and npr.sec_nota_periodo_renglon like nvl(to_char($P{EFP_SEC_NOTA_PERIODO}),'%')
            and (npr.ano||lpad(npr.mes, 2, '0') = $P{EFP_ANO_BASE}||$P{EFP_MES_BASE} or sdr.ano||lpad(sdr.mes, 2, '0') = $P{EFP_ANO_COMP}||$P{EFP_MES_COMP})
    group by
        npr.sec_renglon,
        sdr.sec_renglon,
        npr.sec_nota_periodo_renglon,
        sdr.unidad,
        uni.nombre_centro_costo,
        gcr.sec_grupo_contable,
        gc.nombre
    ) rst
  inner join efv_renglon ren
    on ren.sec_renglon = rst.sec_renglon
  inner join eft_estado_financiero efi
    on efi.sec_estado_financiero = ren.sec_estado_financiero
  inner join organizacion org
    on org.organizacion = NVL($P{EFP_ORGANIZACION},org.organizacion)
  inner join eft_nota_periodo_renglon npr
    on npr.sec_nota_periodo_renglon = rst.sec_nota_periodo_renglon
  left outer join eft_efecto_impresion eim
    on eim.sec_efecto_impresion = ren.sec_efecto_impresion
where org.organizacion = NVL($P{EFP_ORGANIZACION},org.organizacion)
  and efi.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
    and ((rst.grupo < 3) or (rst.saldo_final_base <> 0 or rst.saldo_final_comp <> 0))
order by rst.orden, rst.grupo, rst.codigo_nivel_3, rst.codigo_nivel_4]]>
	</queryString>
