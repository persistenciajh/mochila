	<variable name="V_Tabulador1" class="java.lang.String">
		<variableExpression><![CDATA[$F{NIVEL}.intValue()==1?
        "&nbsp; &nbsp; &nbsp;"
        :$F{NIVEL}.intValue()==2?
            "&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;"
            :$F{NIVEL}.intValue()==3?
                "&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;"
                :$F{NIVEL}.intValue()==4?
                    "&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;"
                    :$F{NIVEL}.intValue()==5?
