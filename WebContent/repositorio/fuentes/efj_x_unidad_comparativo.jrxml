	<queryString>
		<![CDATA[WITH TOTAL_GRUPO_CONTABLE AS (
        SELECT
            SDG.SEC_Grupo_Contable,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} - 1 || $P{EFP_MES_COMP}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Comp_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} - 1 || $P{EFP_MES_COMP}), SDG.Debito, 0)) as Debito_Comp_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} - 1 || $P{EFP_MES_COMP}), SDG.Credito, 0)) as Credito_Comp_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} - 1 || $P{EFP_MES_COMP}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Comp_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} || $P{EFP_MES_COMP}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Comp,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} || $P{EFP_MES_COMP}), SDG.Debito, 0)) as Debito_Comp,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} || $P{EFP_MES_COMP}), SDG.Credito, 0)) as Credito_Comp,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_COMP} || $P{EFP_MES_COMP}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Comp,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Debito, 0)) as Debito_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Credito, 0)) as Credito_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} - 1 || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Ant,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Saldo_Inicial, 0)) as Saldo_Inicial_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Debito, 0)) as Debito_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Credito, 0)) as Credito_Act,
            SUM(DECODE(TO_CHAR(SDG.Ano || lpad(SDG.MES, 2, '0')), TO_CHAR($P{EFP_ANO_BASE} || $P{EFP_MES_BASE}), SDG.Saldo_Inicial + SDG.Debito - SDG.Credito, 0)) as Saldo_Final_Act
        FROM COV_SALDO_DETALLADO_GRUPO SDG
        WHERE ((SDG.ANO = $P{EFP_ANO_BASE} AND lpad(SDG.MES, 2, '0') = $P{EFP_MES_BASE}) OR
               (SDG.ANO = $P{EFP_ANO_BASE} - 1 AND lpad(SDG.MES, 2, '0') = $P{EFP_MES_BASE}) OR
               (SDG.ANO = $P{EFP_ANO_COMP}  and lpad(SDG.MES, 2, '0') = $P{EFP_MES_COMP}) OR
               (SDG.ANO = $P{EFP_ANO_COMP} -1  and lpad(SDG.MES, 2, '0') = $P{EFP_MES_COMP})
              )
        GROUP BY
            SDG.SEC_Grupo_Contable
    ),
    DETALLE_UNIDAD AS (
        select
            ren.orden_estructura,
            ren.nivel,
            ren.sec_renglon,
            ren.mostrar_detalle,
            sdr.unidad,
            sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_BASE}||$P{EFP_MES_BASE}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)) as valor_base,
            sum(decode(to_char(sdr.ano||lpad(sdr.mes, 2, '0')), to_char($P{EFP_ANO_COMP}||$P{EFP_MES_COMP}), (sdr.saldo_inicial + sdr.debito - sdr.credito), 0)) as valor_comp
        from efv_renglon ren
            inner join efv_saldo_detallado_renglon sdr
                on sdr.sec_renglon = ren.sec_renglon
                    and (sdr.ano = $P{EFP_ANO_BASE} or sdr.ano = $P{EFP_ANO_COMP})
                        and (lpad(sdr.mes, 2, '0') = $P{EFP_MES_BASE} or lpad(sdr.mes, 2, '0') = $P{EFP_MES_COMP})
        where ren.sec_estado_financiero = $P{EFP_ESTADO_FINANCIERO}
        group by ren.orden_estructura, ren.nivel, ren.sec_renglon, ren.mostrar_detalle, sdr.unidad
    )
SELECT
    1 AS GRUPO,
    REN.ORDEN_ESTRUCTURA,
    REN.NIVEL,
    REN.SEC_ESTADO_FINANCIERO,
    EFI.NOMBRE,
    DECODE(REN.SEPARADOR, 'N', NPR.NOTA_PERIODO, null) as NOTA_PERIODO,
    REN.SEC_RENGLON,
    REN.RENGLON,
    NULL AS UNIDAD,
    REN.SEPARADOR,
    REN.ORIGEN_VALOR,
    REN.MOSTRAR_SALDO,
    REN.MOSTRAR_DETALLE,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(TRA.TOTAL_RENGLON_COMP, 0)/1000, 'M', NVL(TRA.TOTAL_RENGLON_COMP, 0)/1000000, NVL(TRA.TOTAL_RENGLON_COMP, 0)) AS VALOR_COMP,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(TRA.TOTAL_RENGLON_BASE, 0)/1000, 'M', NVL(TRA.TOTAL_RENGLON_BASE, 0)/1000000, NVL(TRA.TOTAL_RENGLON_BASE, 0)) AS VALOR_BASE,
    ABS(NVL(TRA.TOTAL_RENGLON_BASE, 0) - NVL(TRA.TOTAL_RENGLON_COMP, 0)) AS VARIACION,
    NVL(CASE
            WHEN TRA.TOTAL_RENGLON_BASE = 0 AND TRA.TOTAL_RENGLON_COMP = 0 THEN 0
            WHEN TRA.TOTAL_RENGLON_BASE != 0 AND TRA.TOTAL_RENGLON_COMP = 0 THEN 100
            WHEN TRA.TOTAL_RENGLON_BASE = 0 AND TRA.TOTAL_RENGLON_COMP != 0 THEN -100
            ELSE (ABS(NVL(TRA.TOTAL_RENGLON_BASE, 0) - NVL(TRA.TOTAL_RENGLON_COMP, 0)) * 100) / TRA.TOTAL_RENGLON_COMP
        END, 0) AS PORCENTAJE_VARIACION,
    REN.SEC_EFECTO_IMPRESION,
    EIM.NEGRILLA AS AUX_NEGRILLA,
    EIM.ITALICA AS AUX_ITALICA,
    EIM.SUBRAYADO AS AUX_SUBRAYADO,
    EIM.TAMANO_CARACTER AS AUX_TAMANO
FROM EFV_RENGLON REN
    LEFT OUTER JOIN (
        SELECT
            REN.SEC_RENGLON,
            SUM(TRE.TOTAL_RENGLON_COMP) AS TOTAL_RENGLON_COMP,
            SUM(TRE.TOTAL_RENGLON_BASE) AS TOTAL_RENGLON_BASE
        FROM EFV_RENGLON REN
            INNER JOIN EFT_RENGLON_J JER
                ON JER.SEC_RENGLON = REN.SEC_RENGLON
            LEFT OUTER JOIN (
                SELECT
                    RGV.SEC_RENGLON,
                    SUM(DECODE(RGV.CAMBIO_SIGNO, 'S', RGV.TOTAL_GRUPO_COMP * -1, RGV.TOTAL_GRUPO_COMP)) AS TOTAL_RENGLON_COMP,
                    SUM(DECODE(RGV.CAMBIO_SIGNO, 'S', RGV.TOTAL_GRUPO_BASE * -1, RGV.TOTAL_GRUPO_BASE)) AS TOTAL_RENGLON_BASE
                FROM (
                    SELECT
                        GCR.SEC_RENGLON,
                        TGC.SEC_GRUPO_CONTABLE,
                        GCO.CAMBIO_SIGNO,
                        SUM(DECODE(GCR.TIPO_VALOR,
                                    'SC', TGC.SALDO_FINAL_COMP,
                                    'VR', TGC.SALDO_FINAL_COMP - TGC.SALDO_FINAL_COMP_ANT,
                                    'MD', TGC.DEBITO_COMP,
                                    'MC', TGC.CREDITO_COMP,
                                    'VA', TGC.DEBITO_COMP - TGC.CREDITO_COMP,
                                    'VP', TGC.CREDITO_COMP - TGC.DEBITO_COMP,
                                    'SA', TGC.SALDO_FINAL_COMP_ANT
                                   )
                           ) AS TOTAL_GRUPO_COMP,
                        SUM(DECODE(GCR.TIPO_VALOR,
                                    'SC', TGC.SALDO_FINAL_ACT,
                                    'VR', TGC.SALDO_FINAL_ACT - TGC.SALDO_FINAL_ANT,
                                    'MD', TGC.DEBITO_ACT,
                                    'MC', TGC.CREDITO_ACT,
                                    'VA', TGC.DEBITO_ACT - TGC.CREDITO_ACT,
                                    'VP', TGC.CREDITO_ACT - TGC.DEBITO_ACT,
                                    'SA', TGC.SALDO_FINAL_ANT
                                   )
                           ) AS TOTAL_GRUPO_BASE
                    FROM TOTAL_GRUPO_CONTABLE TGC
                        INNER JOIN EFT_GRUPO_CONTABLE_RENGLON GCR
                            ON GCR.SEC_GRUPO_CONTABLE = TGC.SEC_GRUPO_CONTABLE
                        INNER JOIN COT_GRUPO_CONTABLE GCO
                            ON GCO.SEC_GRUPO_CONTABLE = GCR.SEC_GRUPO_CONTABLE
                    GROUP BY
                        GCR.SEC_RENGLON,
                        TGC.SEC_GRUPO_CONTABLE,
                        GCO.CAMBIO_SIGNO
                    ) RGV
                GROUP BY RGV.SEC_RENGLON
            ) TRE
                ON TRE.SEC_RENGLON = JER.SEC_RENGLON
        GROUP BY REN.SEC_RENGLON
    ) TRA
        ON TRA.SEC_RENGLON = REN.SEC_RENGLON
    INNER JOIN EFT_ESTADO_FINANCIERO EFI
        ON EFI.SEC_ESTADO_FINANCIERO = REN.SEC_ESTADO_FINANCIERO
    LEFT OUTER JOIN EFT_NOTA_PERIODO_RENGLON NPR
        ON NPR.SEC_RENGLON = REN.SEC_RENGLON
            AND NPR.ANO = $P{EFP_ANO_BASE}
                AND lpad(NPR.MES, 2, '0') = $P{EFP_MES_BASE}
    LEFT OUTER JOIN EFT_EFECTO_IMPRESION EIM
        ON EIM.SEC_EFECTO_IMPRESION = REN.SEC_EFECTO_IMPRESION
WHERE EFI.SEC_ESTADO_FINANCIERO = $P{EFP_ESTADO_FINANCIERO}

UNION ALL

SELECT
    2 AS GRUPO,
    DUN.ORDEN_ESTRUCTURA,
    DUN.NIVEL + 1 AS NIVEL,
    NULL AS SEC_ESTADO_FINANCIERO,
    NULL AS NOMBRE,
    NULL AS NOTA_PERIODO,
    NULL AS SEC_RENGLON,
    NULL AS RENGLON,
    '[' || DUN.UNIDAD || '] ' || CCO.NOMBRE_CENTRO_COSTO AS UNIDAD,
    NULL AS SEPARADOR,
    NULL AS ORIGEN_VALOR,
    NULL AS MOSTRAR_SALDO,
    DUN.MOSTRAR_DETALLE AS MOSTRAR_DETALLE,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(DUN.VALOR_COMP, 0)/1000, 'M', NVL(DUN.VALOR_COMP, 0)/1000000, NVL(DUN.VALOR_COMP, 0)) AS VALOR_COMP,
    DECODE($P{GEP_PRESENTACION_CIFRAS}, 'K', NVL(DUN.VALOR_BASE, 0)/1000, 'M', NVL(DUN.VALOR_BASE, 0)/1000000, NVL(DUN.VALOR_BASE, 0)) AS VALOR_BASE,
    ABS(NVL(DUN.VALOR_BASE, 0) - NVL(DUN.VALOR_COMP, 0)) AS VARIACION,
    NVL(CASE
            WHEN DUN.VALOR_BASE = 0 AND DUN.VALOR_COMP = 0 THEN 0
            WHEN DUN.VALOR_BASE != 0 AND DUN.VALOR_COMP = 0 THEN 100
            WHEN DUN.VALOR_BASE = 0 AND DUN.VALOR_COMP != 0 THEN -100
            ELSE (ABS(NVL(DUN.VALOR_BASE, 0) - NVL(DUN.VALOR_COMP, 0)) * 100) / DUN.VALOR_COMP
        END, 0) AS PORCENTAJE_VARIACION,
    NULL AS SEC_EFECTO_IMPRESION,
    NULL AS AUX_NEGRILLA,
    NULL AS AUX_ITALICA,
    NULL AS AUX_SUBRAYADO,
    NULL AS AUX_TAMANO
FROM DETALLE_UNIDAD DUN
    INNER JOIN CENTRO_COSTO CCO
        ON CCO.CENTRO_COSTO = DUN.UNIDAD
WHERE (DUN.VALOR_BASE <> 0 OR DUN.VALOR_COMP <> 0)
    AND DUN.MOSTRAR_DETALLE = 'S'
ORDER BY 2, 1, 9]]>
	</queryString>
